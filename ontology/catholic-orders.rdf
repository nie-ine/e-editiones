    <rdfs:comment xml:lang="en">.</rdfs:comment>
    <rdfs:label xml:lang="en">Order of Carmelites</rdfs:label>
  </rdfs:Class>
  <owl:ObjectProperty rdf:about="http://e-editiones.ch/ontology/catholic-orders#isMemberOf">
    <rdfs:range rdf:resource="http://e-editiones.ch/ontology/catholic-orders#Order"/>
    <rdfs:domain rdf:resource="http://e-editiones.ch/ontology/catholic-orders#Person"/>
    <rdfs:subPropertyOf rdf:resource="http://e-editiones.ch/ontology/catholic-organization#isMemberOf"/>
    <rdfs:comment xml:lang="en">Relating a catholic person to a catholic order the person is a member of.</rdfs:comment>
    <rdfs:label xml:lang="de">ist Mitglied einem katholischen Orden</rdfs:label>
    <rdfs:label xml:lang="en">is member of catholic order</rdfs:label>
    <rdfs:isDefinedBy rdf:resource="http://e-editiones.ch/ontology/catholic-orders#"/>
  </owl:ObjectProperty>

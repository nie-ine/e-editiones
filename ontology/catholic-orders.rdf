  <owl:Ontology rdf:about="http://e-editiones.ch/ontology/catholic-orders">
    <dct:title xml:lang="en">An ontology about catholic orders</dct:title>
    <dct:creator xml:lang="en">Hans Cools, MD, knowledge engineer, ontologist, software developer, Basel University, Switzerland</dct:creator>
    <dct:license rdf:resource="http://creativecommons.org/licenses/by/3.0/"/>
    <dct:description xml:lang="en">Formal description of catholic orders, general subclasses, related classes, properties, and instances.</dct:description>
    <dct:contributor xml:lang="en">Prof. Dr. Ueli Zahnd, Faculty of Theology, University of Basel, Switzerland</dct:contributor>
    <dct:publisher xml:lang="en">Universities of Basel, Bern, and Zürich, Switzerland</dct:publisher>
    <owl:versionInfo rdf:datatype="http://www.w3.org/2001/XMLSchema#date">2020-06-11</owl:versionInfo>
    <dct:contributor xml:lang="en">Gabriel Müller, Faculty of Theology, University of Basel, Switzerland</dct:contributor>
  </owl:Ontology>
  <rdfs:Class rdf:about="http://e-editiones.ch/ontology/catholic-orders#ConventualFranciscans">

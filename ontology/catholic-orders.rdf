    <rdfs:isDefinedBy rdf:resource="http://e-editiones.ch/ontology/catholic-orders#"/>
    <rdfs:label xml:lang="en">Order of Conventual Franciscans</rdfs:label>
    <rdfs:label xml:lang="la">Ordo Fratrum Minorum Conventualium</rdfs:label>
    <rdfs:subClassOf rdf:resource="http://e-editiones.ch/ontology/catholic-orders#Franciscans"/>
  </rdfs:Class>
  <rdfs:Class rdf:about="http://e-editiones.ch/ontology/catholic-orders#Carmelites">

>
  <rdfs:Class rdf:about="http://www.e-editiones.ch/ontology/linguistic-morphology#Punctuation">
    <rdfs:comment xml:lang="en">Part of speech describing the non letter or number based parts of a sentence.</rdfs:comment>
    <rdfs:label xml:lang="de">Interpunktion</rdfs:label>
    <rdfs:subClassOf rdf:resource="http://www.e-editiones.ch/ontology/linguistic-morphology#PartOfSpeech"/>
    <rdfs:label xml:lang="en">punctuation</rdfs:label>
    <rdfs:isDefinedBy rdf:resource="http://www.e-editiones.ch/ontology/linguistic-morphology#"/>
  </rdfs:Class>
  <owl:ObjectProperty rdf:about="http://www.e-editiones.ch/ontology/linguistic-morphology#hasVerbalVoice">
    <rdfs:label xml:lang="de">hat Diathese</rdfs:label>
    <rdfs:label xml:lang="en">has verbal voice</rdfs:label>
    <rdfs:range rdf:resource="http://www.e-editiones.ch/ontology/linguistic-morphology#VerbalVoice"/>
    <rdfs:domain rdf:resource="http://www.e-editiones.ch/ontology/linguistic-morphology#SyntacticWordForm"/>
    <rdfs:comment xml:lang="en">Relating a verbform to a voice.</rdfs:comment>
    <rdfs:isDefinedBy rdf:resource="http://www.e-editiones.ch/ontology/linguistic-morphology#"/>
  </owl:ObjectProperty>
  <owl:ObjectProperty rdf:about="http://www.e-editiones.ch/ontology/linguistic-morphology#hasVerbalMood">
    <rdfs:isDefinedBy rdf:resource="http://www.e-editiones.ch/ontology/linguistic-morphology#"/>
    <rdfs:label xml:lang="en">has verbal mode</rdfs:label>
    <rdfs:label xml:lang="de">hat Modus</rdfs:label>
    <rdfs:range rdf:resource="http://www.e-editiones.ch/ontology/linguistic-morphology#VerbalMood"/>
    <rdfs:comment xml:lang="en">Relating a verbform to mood.</rdfs:comment>
    <rdfs:domain rdf:resource="http://www.e-editiones.ch/ontology/linguistic-morphology#SyntacticWordForm"/>
  </owl:ObjectProperty>
  <rdfs:Class rdf:about="http://www.e-editiones.ch/ontology/linguistic-morphology#MasculineGrammaticalGender">
    <rdfs:label xml:lang="de">grammatisches Maskulinum</rdfs:label>
    <rdfs:isDefinedBy rdf:resource="http://www.e-editiones.ch/ontology/linguistic-morphology#"/>
    <owl:sameAs rdf:resource="http://purl.org/linguistics/gold/MasculineGender"/>
    <rdfs:label xml:lang="en">masculine grammatical gender</rdfs:label>
    <skos:note xml:lang="en">E.g. in Indoeuropean and Semitic languages.</skos:note>
    <rdfs:comment xml:lang="en">Grammatical gender being masculine.</rdfs:comment>
    <rdfs:subClassOf rdf:resource="http://www.e-editiones.ch/ontology/linguistic-morphology#GrammaticalGender"/>
  </rdfs:Class>
  <rdfs:Class rdf:about="http://www.e-editiones.ch/ontology/linguistic-morphology#Present">
    <rdfs:label xml:lang="en">present</rdfs:label>
    <rdfs:isDefinedBy rdf:resource="http://www.e-editiones.ch/ontology/linguistic-morphology#"/>
    <rdfs:label xml:lang="de">Präsens</rdfs:label>
    <rdfs:comment xml:lang="en">...</rdfs:comment>
    <rdfs:subClassOf rdf:resource="http://www.e-editiones.ch/ontology/linguistic-morphology#Tense"/>
  </rdfs:Class>
  <rdfs:Class rdf:about="http://www.e-editiones.ch/ontology/linguistic-morphology#PassiveVoice">
    <rdfs:label xml:lang="en">passive voice</rdfs:label>
    <rdfs:label xml:lang="de">passiv</rdfs:label>
    <rdfs:comment xml:lang="en">Voice that (prototypically) sets the subject as the patient.</rdfs:comment>
    <rdfs:subClassOf rdf:resource="http://www.e-editiones.ch/ontology/linguistic-morphology#VerbalVoice"/>
    <rdfs:isDefinedBy rdf:resource="http://www.e-editiones.ch/ontology/linguistic-morphology#"/>
  </rdfs:Class>
  <rdfs:Class rdf:about="http://www.e-editiones.ch/ontology/linguistic-morphology#Comparative">
    <rdfs:label xml:lang="en">comparative</rdfs:label>
    <rdfs:comment xml:lang="en">Degree of adjective comparison that sets two or more entities into relation.</rdfs:comment>
    <rdfs:label xml:lang="de">Komparativ</rdfs:label>
    <rdfs:isDefinedBy rdf:resource="http://www.e-editiones.ch/ontology/linguistic-morphology#"/>
    <rdfs:subClassOf rdf:resource="http://www.e-editiones.ch/ontology/linguistic-morphology#ComparisonDegree"/>
  </rdfs:Class>
  <rdfs:Class rdf:about="http://www.e-editiones.ch/ontology/linguistic-morphology#ImperativeMood">
    <rdfs:label xml:lang="de">Imperativ</rdfs:label>
    <rdfs:isDefinedBy rdf:resource="http://www.e-editiones.ch/ontology/linguistic-morphology#"/>
    <rdfs:label xml:lang="en">imperative mood</rdfs:label>
    <rdfs:comment xml:lang="en">Verbal mood expressing orders.</rdfs:comment>
    <rdfs:subClassOf rdf:resource="http://www.e-editiones.ch/ontology/linguistic-morphology#VerbalMood"/>
  </rdfs:Class>
  <rdfs:Class rdf:about="http://www.e-editiones.ch/ontology/linguistic-morphology#PersonalPronoun">
    <rdfs:label xml:lang="en">personal pronoun</rdfs:label>
    <rdfs:subClassOf rdf:resource="http://www.e-editiones.ch/ontology/linguistic-morphology#Pronoun"/>
    <rdfs:label xml:lang="de">Personalpronomen</rdfs:label>
    <rdfs:comment xml:lang="en">Pronoun referring to a person, and declined by person, gender, number, case and formality.</rdfs:comment>
    <rdfs:isDefinedBy rdf:resource="http://www.e-editiones.ch/ontology/linguistic-morphology#"/>
  </rdfs:Class>
  <rdfs:Class rdf:about="http://www.e-editiones.ch/ontology/linguistic-morphology#Postposition">
    <rdfs:label xml:lang="de">Postposition</rdfs:label>
    <rdfs:subClassOf rdf:resource="http://www.e-editiones.ch/ontology/linguistic-morphology#Adposition"/>
    <rdfs:isDefinedBy rdf:resource="http://www.e-editiones.ch/ontology/linguistic-morphology#"/>
    <rdfs:comment xml:lang="en">Part of speech indicating relationship to objects, standing after them.</rdfs:comment>
    <rdfs:label xml:lang="en">postposition</rdfs:label>
  </rdfs:Class>
  <rdfs:Class rdf:about="http://www.e-editiones.ch/ontology/linguistic-morphology#SubordinatingConjunction">
    <rdfs:isDefinedBy rdf:resource="http://www.e-editiones.ch/ontology/linguistic-morphology#"/>
    <rdfs:comment xml:lang="en">Part of speech that orders one syntactic entity under another.</rdfs:comment>
    <rdfs:label xml:lang="de">unterordnende Konjunktion, Subjunktion</rdfs:label>
    <rdfs:subClassOf rdf:resource="http://www.e-editiones.ch/ontology/linguistic-morphology#Conjunction"/>
    <rdfs:label xml:lang="en">subordinating conjunction</rdfs:label>
  </rdfs:Class>
  <rdfs:Class rdf:about="http://www.e-editiones.ch/ontology/linguistic-morphology#VocativeCase">
    <rdfs:label xml:lang="en">vocative case</rdfs:label>
    <rdfs:subClassOf rdf:resource="http://www.e-editiones.ch/ontology/linguistic-morphology#Case"/>
    <rdfs:isDefinedBy rdf:resource="http://www.e-editiones.ch/ontology/linguistic-morphology#"/>
    <rdfs:comment xml:lang="en">Grammatical case of a noun or pronoun used to address a person in grammatical sense.</rdfs:comment>
    <rdfs:label xml:lang="de">Vokativ</rdfs:label>
  </rdfs:Class>
  <rdfs:Class rdf:about="http://www.e-editiones.ch/ontology/linguistic-morphology#CardinalNumeral">
    <rdfs:comment xml:lang="en">Part of speech describing numbers as a measure.</rdfs:comment>
    <rdfs:isDefinedBy rdf:resource="http://www.e-editiones.ch/ontology/linguistic-morphology#"/>
    <rdfs:label xml:lang="de">Kardinalzahl</rdfs:label>
    <rdfs:subClassOf rdf:resource="http://www.e-editiones.ch/ontology/linguistic-morphology#Numeral"/>
    <rdfs:label xml:lang="en">cardinal numeral</rdfs:label>
  </rdfs:Class>
  <rdfs:Class rdf:about="http://www.e-editiones.ch/ontology/linguistic-morphology#Suffix">
    <rdfs:isDefinedBy rdf:resource="http://www.e-editiones.ch/ontology/linguistic-morphology#"/>
    <rdfs:label xml:lang="en">suffix</rdfs:label>
    <rdfs:comment xml:lang="en">Affix at the end of a word, e.g. 'ship' in 'friendship'.</rdfs:comment>
    <rdfs:subClassOf rdf:resource="http://www.e-editiones.ch/ontology/linguistic-morphology#Affix"/>
    <rdfs:label xml:lang="de">Suffix</rdfs:label>
  </rdfs:Class>
  <rdfs:Class rdf:about="http://www.e-editiones.ch/ontology/linguistic-morphology#FirstPerson">
    <rdfs:isDefinedBy rdf:resource="http://www.e-editiones.ch/ontology/linguistic-morphology#"/>
    <rdfs:label xml:lang="de">1. Person</rdfs:label>
    <rdfs:label xml:lang="en">1st person</rdfs:label>
    <rdfs:comment xml:lang="en">Mentioned entitiy overlaps with speaker.</rdfs:comment>
    <rdfs:subClassOf rdf:resource="http://www.e-editiones.ch/ontology/linguistic-morphology#Person"/>
  </rdfs:Class>
  <rdfs:Class rdf:about="http://www.e-editiones.ch/ontology/linguistic-morphology#RelativePronoun">
    <rdfs:isDefinedBy rdf:resource="http://www.e-editiones.ch/ontology/linguistic-morphology#"/>
    <rdfs:label xml:lang="en">relative pronoun</rdfs:label>
    <rdfs:label xml:lang="de">Relativpronomen</rdfs:label>
    <rdfs:comment xml:lang="en">Pronoun introducing a relative clause.</rdfs:comment>
    <rdfs:subClassOf rdf:resource="http://www.e-editiones.ch/ontology/linguistic-morphology#Pronoun"/>
  </rdfs:Class>
  <owl:ObjectProperty rdf:about="http://www.e-editiones.ch/ontology/linguistic-morphology#hasNumber">
    <rdfs:label xml:lang="en">syntactic word form has number</rdfs:label>
    <rdfs:label xml:lang="de">syntactische Wortform hat Numerus</rdfs:label>
    <rdfs:domain rdf:resource="http://www.e-editiones.ch/ontology/linguistic-morphology#SyntacticWordForm"/>
    <rdfs:isDefinedBy rdf:resource="http://www.e-editiones.ch/ontology/linguistic-morphology#"/>
    <rdfs:comment xml:lang="en">Relating a syntactic word to its number.</rdfs:comment>
    <rdfs:range rdf:resource="http://www.e-editiones.ch/ontology/linguistic-morphology#Number"/>
  </owl:ObjectProperty>
  <owl:ObjectProperty rdf:about="http://www.e-editiones.ch/ontology/linguistic-morphology#hasPerson">
    <rdfs:isDefinedBy rdf:resource="http://www.e-editiones.ch/ontology/linguistic-morphology#"/>
    <rdfs:label xml:lang="de">hat Person</rdfs:label>
    <rdfs:label xml:lang="en">has person</rdfs:label>
    <rdfs:comment xml:lang="en">Relating a wordform to a person property.</rdfs:comment>
    <rdfs:range rdf:resource="http://www.e-editiones.ch/ontology/linguistic-morphology#Person"/>
    <rdfs:domain rdf:resource="http://www.e-editiones.ch/ontology/linguistic-morphology#SyntacticWordForm"/>
  </owl:ObjectProperty>
  <rdfs:Class rdf:about="http://www.e-editiones.ch/ontology/linguistic-morphology#OptativeMood">
    <rdfs:subClassOf rdf:resource="http://www.e-editiones.ch/ontology/linguistic-morphology#VerbalMood"/>
    <rdfs:label xml:lang="de">Optativ</rdfs:label>
    <rdfs:isDefinedBy rdf:resource="http://www.e-editiones.ch/ontology/linguistic-morphology#"/>
    <rdfs:label xml:lang="en">optative mood</rdfs:label>
    <rdfs:comment xml:lang="en">Verbal mood indicating wishes.</rdfs:comment>
  </rdfs:Class>
  <rdfs:Class rdf:about="http://www.e-editiones.ch/ontology/linguistic-morphology#GermanCase">
    <rdfs:label xml:lang="de">deutscher Kasus</rdfs:label>
    <rdfs:isDefinedBy rdf:resource="http://www.e-editiones.ch/ontology/linguistic-morphology#"/>
    <rdfs:subClassOf rdf:resource="http://www.e-editiones.ch/ontology/linguistic-morphology#Case"/>
    <rdfs:comment xml:lang="en">German case form of a word.</rdfs:comment>
    <rdfs:label xml:lang="en">German case</rdfs:label>
  </rdfs:Class>
  <rdfs:Class rdf:about="http://www.e-editiones.ch/ontology/linguistic-morphology#ThirdPerson">
    <rdfs:isDefinedBy rdf:resource="http://www.e-editiones.ch/ontology/linguistic-morphology#"/>
    <rdfs:comment xml:lang="en">Degree of comparison in adjectives or adverbs.</rdfs:comment>
    <rdfs:label xml:lang="de">3. Person</rdfs:label>
    <rdfs:label xml:lang="en">3rd person</rdfs:label>
    <rdfs:subClassOf rdf:resource="http://www.e-editiones.ch/ontology/linguistic-morphology#Person"/>
  </rdfs:Class>
  <rdfs:Class rdf:about="http://www.e-editiones.ch/ontology/linguistic-morphology#SemanticGender">
    <rdfs:isDefinedBy rdf:resource="http://www.e-editiones.ch/ontology/linguistic-morphology#"/>
    <rdfs:label xml:lang="en">semantic gender</rdfs:label>
    <rdfs:subClassOf rdf:resource="http://www.e-editiones.ch/ontology/linguistics#Concept"/>
    <rdfs:label xml:lang="de">semantisches Genus</rdfs:label>
    <rdfs:comment xml:lang="en">Semantic gender of a lexeme.</rdfs:comment>
  </rdfs:Class>
  <rdfs:Class rdf:about="http://www.e-editiones.ch/ontology/linguistic-morphology#NegativeParticle">
    <rdfs:comment xml:lang="en">Part of speech designating non traditionally accepted parts of speech with strong grammatical function.</rdfs:comment>
    <rdfs:subClassOf rdf:resource="http://www.e-editiones.ch/ontology/linguistic-morphology#PartOfSpeech"/>
    <rdfs:isDefinedBy rdf:resource="http://www.e-editiones.ch/ontology/linguistic-morphology#"/>
    <rdfs:label xml:lang="de">Negationspartikel</rdfs:label>
    <rdfs:label xml:lang="en">negative particle</rdfs:label>
  </rdfs:Class>
  <rdfs:Class rdf:about="http://www.e-editiones.ch/ontology/linguistic-morphology#Imperfect">
    <rdfs:label xml:lang="de">Imperfekt</rdfs:label>
    <rdfs:isDefinedBy rdf:resource="http://www.e-editiones.ch/ontology/linguistic-morphology#"/>
    <rdfs:subClassOf rdf:resource="http://www.e-editiones.ch/ontology/linguistic-morphology#Tense"/>
    <rdfs:label xml:lang="en">imperfect</rdfs:label>
    <rdfs:comment xml:lang="en">...</rdfs:comment>
  </rdfs:Class>
  <rdfs:Class rdf:about="http://www.e-editiones.ch/ontology/linguistic-morphology#ConditionalMood">
    <rdfs:subClassOf rdf:resource="http://www.e-editiones.ch/ontology/linguistic-morphology#VerbalMood"/>
    <rdfs:isDefinedBy rdf:resource="http://www.e-editiones.ch/ontology/linguistic-morphology#"/>
    <rdfs:label xml:lang="de">Konditional</rdfs:label>
    <rdfs:comment xml:lang="en">Verbal mood expressiong conditions.</rdfs:comment>
    <rdfs:label xml:lang="en">conditional mood</rdfs:label>
  </rdfs:Class>
  <rdfs:Class rdf:about="http://www.e-editiones.ch/ontology/linguistic-morphology#FeminineGrammaticalGender">
    <owl:sameAs rdf:resource="http://purl.org/linguistics/gold/FeminineGender"/>
    <rdfs:label xml:lang="en">feminine grammatical gender</rdfs:label>
    <rdfs:isDefinedBy rdf:resource="http://www.e-editiones.ch/ontology/linguistic-morphology#"/>
    <rdfs:label xml:lang="de">grammatisches Femininum</rdfs:label>
    <rdfs:subClassOf rdf:resource="http://www.e-editiones.ch/ontology/linguistic-morphology#GrammaticalGender"/>
    <skos:note xml:lang="en">E.g. in Indoeuropean and Semitic languages.</skos:note>
    <rdfs:comment xml:lang="en">Grammatical gender being feminine.</rdfs:comment>
  </rdfs:Class>
  <rdfs:Class rdf:about="http://www.e-editiones.ch/ontology/linguistic-morphology#Pluperfect">
    <rdfs:comment xml:lang="en">...</rdfs:comment>
    <rdfs:label xml:lang="de">Plusquamperfekt</rdfs:label>
    <rdfs:isDefinedBy rdf:resource="http://www.e-editiones.ch/ontology/linguistic-morphology#"/>
    <rdfs:label xml:lang="en">pluperfect</rdfs:label>
    <rdfs:subClassOf rdf:resource="http://www.e-editiones.ch/ontology/linguistic-morphology#Tense"/>
  </rdfs:Class>
  <rdfs:Class rdf:about="http://www.e-editiones.ch/ontology/linguistic-morphology#MedialVoice">
    <rdfs:comment xml:lang="en">.</rdfs:comment>
    <rdfs:label xml:lang="en">medial voice</rdfs:label>
    <rdfs:subClassOf rdf:resource="http://www.e-editiones.ch/ontology/linguistic-morphology#VerbalVoice"/>
    <rdfs:label xml:lang="de">medial</rdfs:label>
    <rdfs:isDefinedBy rdf:resource="http://www.e-editiones.ch/ontology/linguistic-morphology#"/>
  </rdfs:Class>
  <rdfs:Class rdf:about="http://www.e-editiones.ch/ontology/linguistic-morphology#SecondPerson">
    <rdfs:isDefinedBy rdf:resource="http://www.e-editiones.ch/ontology/linguistic-morphology#"/>
    <rdfs:comment xml:lang="en">Mentioned entity overlaps with listener.</rdfs:comment>
    <rdfs:label xml:lang="en">2nd person</rdfs:label>
    <rdfs:subClassOf rdf:resource="http://www.e-editiones.ch/ontology/linguistic-morphology#Person"/>
    <rdfs:label xml:lang="de">2. Person</rdfs:label>
  </rdfs:Class>
  <rdfs:Class rdf:about="http://www.e-editiones.ch/ontology/linguistic-morphology#Plural">
    <rdfs:subClassOf rdf:resource="http://www.e-editiones.ch/ontology/linguistic-morphology#Number"/>
    <rdfs:comment xml:lang="en">Number of a word indicating a plurality.</rdfs:comment>
    <rdfs:label xml:lang="de">Plural</rdfs:label>
    <rdfs:isDefinedBy rdf:resource="http://www.e-editiones.ch/ontology/linguistic-morphology#"/>
    <rdfs:label xml:lang="en">plural</rdfs:label>
  </rdfs:Class>
  <rdfs:Class rdf:about="http://www.e-editiones.ch/ontology/linguistic-morphology#DativeCase">
    <rdfs:label xml:lang="de">Dativ</rdfs:label>
    <rdfs:comment xml:lang="en">Grammatical case indicating an indirect object.</rdfs:comment>
    <rdfs:isDefinedBy rdf:resource="http://www.e-editiones.ch/ontology/linguistic-morphology#"/>
    <rdfs:label xml:lang="en">dative case</rdfs:label>
    <rdfs:subClassOf rdf:resource="http://www.e-editiones.ch/ontology/linguistic-morphology#Case"/>
  </rdfs:Class>
  <rdfs:Class rdf:about="http://www.e-editiones.ch/ontology/linguistic-morphology#Aorist">
    <rdfs:subClassOf rdf:resource="http://www.e-editiones.ch/ontology/linguistic-morphology#Tense"/>
    <rdfs:label xml:lang="en">aorist</rdfs:label>
    <rdfs:isDefinedBy rdf:resource="http://www.e-editiones.ch/ontology/linguistic-morphology#"/>
    <rdfs:comment xml:lang="en">...</rdfs:comment>
    <rdfs:label xml:lang="de">Aorist</rdfs:label>
  </rdfs:Class>
  <rdfs:Class rdf:about="http://www.e-editiones.ch/ontology/linguistic-morphology#ProperNoun">
    <rdfs:comment xml:lang="en">Part of speech representing an instance.</rdfs:comment>
    <rdfs:label xml:lang="en">proper noun</rdfs:label>
    <skos:note xml:lang="en">Written with starting capital, for naming, e.g. a person, a geographical entity (e.g. a country); in English and German also a calendar entity (e.g. resp. 'Monday', 'Montag')</skos:note>
    <rdfs:isDefinedBy rdf:resource="http://www.e-editiones.ch/ontology/linguistic-morphology#"/>
    <rdfs:subClassOf rdf:resource="http://www.e-editiones.ch/ontology/linguistic-morphology#Noun"/>
    <rdfs:label xml:lang="de">Eigenname</rdfs:label>
  </rdfs:Class>

    <rdfs:range rdf:resource="http://www.e-editiones.ch/ontology/linguistic-morphology#Case"/>
    <rdfs:isDefinedBy rdf:resource="http://www.e-editiones.ch/ontology/linguistic-morphology#"/>
  </owl:ObjectProperty>
  <rdfs:Class rdf:about="http://www.e-editiones.ch/ontology/linguistic-morphology#ReflexivePronoun">
    <rdfs:label xml:lang="de">Reflexivpronomen</rdfs:label>
    <rdfs:comment xml:lang="en">Pronoun pointing back to the subject of the sentence, e.g. 'yourself'.</rdfs:comment>
    <rdfs:isDefinedBy rdf:resource="http://www.e-editiones.ch/ontology/linguistic-morphology#"/>
    <rdfs:label xml:lang="en">reflexive pronoun</rdfs:label>
    <rdfs:subClassOf rdf:resource="http://www.e-editiones.ch/ontology/linguistic-morphology#Pronoun"/>
  </rdfs:Class>
  <rdfs:Class rdf:about="http://www.e-editiones.ch/ontology/linguistic-morphology#PrecativeMood">
    <rdfs:label xml:lang="en">precative mood</rdfs:label>
    <rdfs:label xml:lang="de">Präkativ</rdfs:label>
    <rdfs:comment xml:lang="en">Verbal mood ... .</rdfs:comment>
    <rdfs:isDefinedBy rdf:resource="http://www.e-editiones.ch/ontology/linguistic-morphology#"/>
    <rdfs:subClassOf rdf:resource="http://www.e-editiones.ch/ontology/linguistic-morphology#VerbalMood"/>
  </rdfs:Class>
  <rdfs:Class rdf:about="http://www.e-editiones.ch/ontology/linguistic-morphology#IndicativeMood">
    <rdfs:label xml:lang="en">indicative mood</rdfs:label>
    <rdfs:comment xml:lang="en">Verbal mood for stating facts.</rdfs:comment>
    <rdfs:isDefinedBy rdf:resource="http://www.e-editiones.ch/ontology/linguistic-morphology#"/>
    <rdfs:subClassOf rdf:resource="http://www.e-editiones.ch/ontology/linguistic-morphology#VerbalMood"/>
    <rdfs:label xml:lang="de">Indikativ</rdfs:label>
  </rdfs:Class>
  <rdfs:Class rdf:about="http://www.e-editiones.ch/ontology/linguistic-morphology#AblativeCase">
    <rdfs:isDefinedBy rdf:resource="http://www.e-editiones.ch/ontology/linguistic-morphology#"/>
    <rdfs:comment xml:lang="en">Grammatical case specifying a separation.</rdfs:comment>
    <rdfs:label xml:lang="de">Ablativ</rdfs:label>
    <rdfs:subClassOf rdf:resource="http://www.e-editiones.ch/ontology/linguistic-morphology#Case"/>
    <rdfs:label xml:lang="en">ablative case</rdfs:label>
  </rdfs:Class>
  <rdfs:Class rdf:about="http://www.e-editiones.ch/ontology/linguistic-morphology#InterrogativePronoun">
    <rdfs:label xml:lang="en">interrogative pronoun</rdfs:label>
    <rdfs:isDefinedBy rdf:resource="http://www.e-editiones.ch/ontology/linguistic-morphology#"/>
    <rdfs:comment xml:lang="en">Pronoun introducing a question.</rdfs:comment>
    <rdfs:label xml:lang="de">Interrogativpronomen</rdfs:label>
    <rdfs:subClassOf rdf:resource="http://www.e-editiones.ch/ontology/linguistic-morphology#Pronoun"/>
  </rdfs:Class>
  <rdfs:Class rdf:about="http://www.e-editiones.ch/ontology/linguistic-morphology#Affix">
    <rdfs:label xml:lang="de">Affix</rdfs:label>
    <rdfs:isDefinedBy rdf:resource="http://www.e-editiones.ch/ontology/linguistic-morphology#"/>
    <rdfs:subClassOf rdf:resource="http://www.e-editiones.ch/ontology/linguistic-morphology#Morpheme"/>
    <rdfs:label xml:lang="en">affix</rdfs:label>
    <rdfs:comment xml:lang="en">Bound morpheme without own semantics, depending on a root; part of a word, complement of a word stem.</rdfs:comment>
  </rdfs:Class>
  <rdfs:Class rdf:about="http://www.e-editiones.ch/ontology/linguistic-morphology#VerbalVoice">
    <rdfs:label xml:lang="en">verbal voice</rdfs:label>
    <rdfs:subClassOf rdf:resource="http://www.e-editiones.ch/ontology/linguistics#Concept"/>
    <rdfs:isDefinedBy rdf:resource="http://www.e-editiones.ch/ontology/linguistic-morphology#"/>
    <rdfs:label xml:lang="de">Diathese</rdfs:label>
    <owl:sameAs rdf:resource="http://purl.org/linguistics/gold/VoiceProperty"/>
    <rdfs:comment xml:lang="en">Property of verb forms that specifies the roles of the dependent entities.</rdfs:comment>
  </rdfs:Class>
  <rdfs:Class rdf:about="http://www.e-editiones.ch/ontology/linguistic-morphology#Tense">
    <rdfs:comment xml:lang="en">...</rdfs:comment>
    <rdfs:label xml:lang="en">tense</rdfs:label>
    <rdfs:subClassOf rdf:resource="http://www.e-editiones.ch/ontology/linguistics#Concept"/>
    <rdfs:label xml:lang="de">Tempus</rdfs:label>
    <rdfs:isDefinedBy rdf:resource="http://www.e-editiones.ch/ontology/linguistic-morphology#"/>
  </rdfs:Class>
  <rdfs:Class rdf:about="http://www.e-editiones.ch/ontology/linguistic-morphology#Number">
    <rdfs:label xml:lang="en">number</rdfs:label>
    <rdfs:label xml:lang="de">Numerus</rdfs:label>
    <rdfs:subClassOf rdf:resource="http://www.e-editiones.ch/ontology/linguistics#Concept"/>
    <rdfs:isDefinedBy rdf:resource="http://www.e-editiones.ch/ontology/linguistic-morphology#"/>
    <rdfs:comment xml:lang="en">Grammatical declension of a word expressing a number of a word.</rdfs:comment>
  </rdfs:Class>
  <rdfs:Class rdf:about="http://www.e-editiones.ch/ontology/linguistic-morphology#GrammaticalGender">
    <rdfs:subClassOf rdf:resource="http://www.e-editiones.ch/ontology/linguistics#Concept"/>
    <rdfs:comment xml:lang="en">Grammatical declension of a noun indicating a gender, independent from a semantic gender.</rdfs:comment>
    <rdfs:label xml:lang="en">grammatical gender</rdfs:label>
    <rdfs:isDefinedBy rdf:resource="http://www.e-editiones.ch/ontology/linguistic-morphology#"/>
    <rdfs:label xml:lang="de">grammatisches Genus</rdfs:label>
  </rdfs:Class>
  <rdfs:Class rdf:about="http://www.e-editiones.ch/ontology/linguistic-morphology#Numeral">
    <rdfs:isDefinedBy rdf:resource="http://www.e-editiones.ch/ontology/linguistic-morphology#"/>
    <rdfs:subClassOf rdf:resource="http://www.e-editiones.ch/ontology/linguistic-morphology#PartOfSpeech"/>
    <rdfs:comment xml:lang="en">Part of speech designating a number.</rdfs:comment>
    <rdfs:label xml:lang="en">numeral</rdfs:label>
    <rdfs:label xml:lang="de">Numerale</rdfs:label>
  </rdfs:Class>
  <rdfs:Class rdf:about="http://www.e-editiones.ch/ontology/linguistic-morphology#Article">
    <rdfs:comment xml:lang="en">Part of speech specifying grammatical definiteness, i.e. indicating if an object is known or newly introduced.</rdfs:comment>
    <rdfs:label xml:lang="en">article</rdfs:label>
    <rdfs:isDefinedBy rdf:resource="http://www.e-editiones.ch/ontology/linguistic-morphology#"/>
    <rdfs:label xml:lang="de">Artikel</rdfs:label>
    <rdfs:subClassOf rdf:resource="http://www.e-editiones.ch/ontology/linguistic-morphology#PartOfSpeech"/>
  </rdfs:Class>
  <rdfs:Class rdf:about="http://www.e-editiones.ch/ontology/linguistic-morphology#Case">
    <rdfs:subClassOf rdf:resource="http://www.e-editiones.ch/ontology/linguistics#Concept"/>
    <rdfs:isDefinedBy rdf:resource="http://www.e-editiones.ch/ontology/linguistic-morphology#"/>
    <rdfs:label xml:lang="de">Kasus</rdfs:label>
    <rdfs:label xml:lang="en">grammatical case</rdfs:label>
    <rdfs:comment xml:lang="en">Grammatical declension of a noun, pronoun, adjective, participle, article, or numeral, expressing its grammatical function, hence its semantic relation to other words in a sentence, clause or phrase.</rdfs:comment>
  </rdfs:Class>
  <rdfs:Class rdf:about="http://www.e-editiones.ch/ontology/linguistic-morphology#Adposition">
    <rdfs:label xml:lang="de">Adposition</rdfs:label>
    <rdfs:comment xml:lang="en">Part of speech indicating a relationship to objects.</rdfs:comment>
    <rdfs:subClassOf rdf:resource="http://www.e-editiones.ch/ontology/linguistic-morphology#PartOfSpeech"/>
    <rdfs:label xml:lang="en">adposition</rdfs:label>
    <rdfs:isDefinedBy rdf:resource="http://www.e-editiones.ch/ontology/linguistic-morphology#"/>
  </rdfs:Class>
  <rdfs:Class rdf:about="http://www.e-editiones.ch/ontology/linguistic-morphology#ComparisonDegree">
    <rdfs:comment xml:lang="en">Degree of comparison in adjectives or adverbs.</rdfs:comment>
    <rdfs:label xml:lang="de">Komparationsstufe</rdfs:label>
    <rdfs:subClassOf rdf:resource="http://www.e-editiones.ch/ontology/linguistics#Concept"/>
    <rdfs:label xml:lang="en">comparison degree</rdfs:label>
    <rdfs:isDefinedBy rdf:resource="http://www.e-editiones.ch/ontology/linguistic-morphology#"/>
  </rdfs:Class>
  <rdfs:Class rdf:about="http://www.e-editiones.ch/ontology/linguistic-morphology#Pronoun">
    <rdfs:comment xml:lang="en">Part of speech quantifying or replacing an object.</rdfs:comment>
    <rdfs:label xml:lang="de">Pronomen</rdfs:label>
    <rdfs:isDefinedBy rdf:resource="http://www.e-editiones.ch/ontology/linguistic-morphology#"/>
    <rdfs:subClassOf rdf:resource="http://www.e-editiones.ch/ontology/linguistic-morphology#PartOfSpeech"/>
    <rdfs:label xml:lang="en">pronoun</rdfs:label>
  </rdfs:Class>
  <rdfs:Class rdf:about="http://www.e-editiones.ch/ontology/linguistic-morphology#PartOfSpeech">
    <rdfs:isDefinedBy rdf:resource="http://www.e-editiones.ch/ontology/linguistic-morphology#"/>
    <rdfs:label xml:lang="en">part of speech</rdfs:label>
    <rdfs:subClassOf rdf:resource="http://www.e-editiones.ch/ontology/linguistics#Concept"/>
    <rdfs:label xml:lang="de">Wortart</rdfs:label>
    <rdfs:comment xml:lang="en">Word with certain grammatical properties and syntactic role in the structure of a sentence.</rdfs:comment>
  </rdfs:Class>
  <rdfs:Class rdf:about="http://www.e-editiones.ch/ontology/linguistic-morphology#Morpheme">
    <rdfs:label xml:lang="en">morpheme</rdfs:label>
    <rdfs:label xml:lang="de">Morphem</rdfs:label>
    <rdfs:isDefinedBy rdf:resource="http://www.e-editiones.ch/ontology/linguistic-morphology#"/>
    <rdfs:comment xml:lang="en">Linguistic concept as the smallest entity in human natural language with a grammatical function, inflectable, free or bound.</rdfs:comment>
    <rdfs:subClassOf rdf:resource="http://www.e-editiones.ch/ontology/linguistics#Concept"/>
  </rdfs:Class>
  <rdfs:Class rdf:about="http://www.e-editiones.ch/ontology/linguistic-morphology#Person">
    <rdfs:comment xml:lang="en">Property indicating the relation between an entity in an utterance and the speaker.</rdfs:comment>
    <rdfs:label xml:lang="de">Person</rdfs:label>
    <rdfs:label xml:lang="en">person</rdfs:label>
    <rdfs:isDefinedBy rdf:resource="http://www.e-editiones.ch/ontology/linguistic-morphology#"/>
    <rdfs:subClassOf rdf:resource="http://www.e-editiones.ch/ontology/linguistics#Concept"/>
  </rdfs:Class>
  <rdfs:Class rdf:about="http://www.e-editiones.ch/ontology/linguistic-morphology#Noun">
    <rdfs:subClassOf rdf:resource="http://www.e-editiones.ch/ontology/linguistic-morphology#PartOfSpeech"/>
    <rdfs:comment xml:lang="en">Part of speech representing a class or instance.</rdfs:comment>
    <rdfs:isDefinedBy rdf:resource="http://www.e-editiones.ch/ontology/linguistic-morphology#"/>
    <skos:note xml:lang="en">In German there is no grouping concept for 'Substantiv' and 'Pronomen'.</skos:note>
    <rdfs:label xml:lang="en">noun</rdfs:label>
  </rdfs:Class>
  <rdfs:Class rdf:about="http://www.e-editiones.ch/ontology/linguistic-morphology#VerbalMood">
    <rdfs:label xml:lang="de">verbaler Modus</rdfs:label>
    <rdfs:subClassOf rdf:resource="http://www.e-editiones.ch/ontology/linguistics#Concept"/>
    <rdfs:isDefinedBy rdf:resource="http://www.e-editiones.ch/ontology/linguistic-morphology#"/>
    <rdfs:comment xml:lang="en">Property of verbs indicating real or hypothetic utterances.</rdfs:comment>
    <rdfs:label xml:lang="en">verbal mood</rdfs:label>
  </rdfs:Class>
  <rdfs:Class rdf:about="http://www.e-editiones.ch/ontology/linguistic-morphology#SyntacticWordForm">
    <rdfs:subClassOf rdf:resource="http://www.e-editiones.ch/ontology/linguistics#Concept"/>
    <rdfs:comment xml:lang="en">Linguistic concept unifying occurrences of syntactic words with identical literal and grammemes.</rdfs:comment>
    <rdfs:label xml:lang="en">syntactic word form</rdfs:label>
    <skos:note xml:lang="en">A sentence can contain 2 times the syntactic word 'sun', unified by 1 syntactic word form 'sun'.</skos:note>
    <rdfs:isDefinedBy rdf:resource="http://www.e-editiones.ch/ontology/linguistic-morphology#"/>
    <rdfs:label xml:lang="de">syntaktische Wortform</rdfs:label>
  </rdfs:Class>
  <rdfs:Class rdf:about="http://www.e-editiones.ch/ontology/linguistic-morphology#Conjunction">
    <rdfs:comment xml:lang="en">Part of speech linking syntactic entities.</rdfs:comment>
    <rdfs:isDefinedBy rdf:resource="http://www.e-editiones.ch/ontology/linguistic-morphology#"/>
    <rdfs:label xml:lang="de">Konjunktion</rdfs:label>
    <rdfs:label xml:lang="en">conjunction</rdfs:label>
    <rdfs:subClassOf rdf:resource="http://www.e-editiones.ch/ontology/linguistic-morphology#PartOfSpeech"/>
  </rdfs:Class>
</rdf:RDF>

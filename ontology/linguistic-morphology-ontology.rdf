    <rdfs:isDefinedBy rdf:resource="http://www.e-editiones.ch/ontology/linguistic-morphology#"/>
    <rdfs:subClassOf rdf:resource="http://www.e-editiones.ch/ontology/linguistic-morphology#Case"/>
    <rdfs:label xml:lang="en">genitive case</rdfs:label>
    <rdfs:label xml:lang="de">Genitiv</rdfs:label>
    <rdfs:comment xml:lang="en">Grammatical case indicating possession and similar relations.</rdfs:comment>
  </rdfs:Class>
  <rdfs:Class rdf:about="http://www.e-editiones.ch/ontology/linguistic-morphology#DemonstrativePronoun">

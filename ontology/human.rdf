    <owl:propertyChainAxiom rdf:parseType="Collection">
      <rdf:Description rdf:about="http://e-editiones.ch/ontology/human#hasLife"/>
      <rdf:Description rdf:about="http://e-editiones.ch/ontology/time#hasMaxDuration"/>
    </owl:propertyChainAxiom>
    <rdfs:domain rdf:resource="http://e-editiones.ch/ontology/human#Person"/>
    <rdfs:comment xml:lang="en">Relating a person to a maximum life span the person has.</rdfs:comment>
    <rdfs:label xml:lang="de">hat maximale Lebensdauer</rdfs:label>
  </owl:DatatypeProperty>
  <owl:ObjectProperty rdf:about="http://e-editiones.ch/ontology/human#hasBirthYearPeriod">
    <rdfs:label xml:lang="de">hat Geburtsjahrzeitraum</rdfs:label>
    <skos:note xml:lang="en">Through a series of N3-rules a birth year literal is converted to its year period with the respective calendar start and end date typed literal, and with the start and end Julian Day Number, to enable interval calculus for temporal reasoning.</skos:note>
    <rdfs:domain rdf:resource="http://e-editiones.ch/ontology/human#Person"/>
    <rdfs:range rdf:resource="http://e-editiones.ch/ontology/event#Event"/>
    <rdfs:isDefinedBy rdf:resource="http://e-editiones.ch/ontology/human#"/>
    <rdfs:comment xml:lang="en">Relating a person to the person's birth year period.</rdfs:comment>
    <rdfs:label xml:lang="en">has birth year period</rdfs:label>
  </owl:ObjectProperty>

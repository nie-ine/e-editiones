  <owl:ObjectProperty rdf:about="http://e-editiones.ch/ontology/human#isMemberOf">
    <rdfs:label xml:lang="en">is member of</rdfs:label>
    <rdfs:range rdf:resource="http://e-editiones.ch/ontology/human#Group"/>
    <rdfs:domain rdf:resource="http://e-editiones.ch/ontology/human#Person"/>
    <rdfs:isDefinedBy rdf:resource="http://e-editiones.ch/ontology/human#"/>
    <rdfs:subPropertyOf rdf:resource="http://e-editiones.ch/ontology/agent#isMemberOf"/>
    <rdfs:comment xml:lang="en">Relating a person to a group the person is a member of.</rdfs:comment>
    <rdfs:label xml:lang="de">ist Mitglied</rdfs:label>
  </owl:ObjectProperty>
  <owl:DatatypeProperty rdf:about="http://e-editiones.ch/ontology/human#hasMaxLifeSpan">
    <rdfs:label xml:lang="en">has maximum life span</rdfs:label>

    <rdfs:subClassOf rdf:resource="http://e-editiones.ch/ontology/human#AgentSponsor"/>
    <rdfs:comment xml:lang="en">Role of a person as a sponsor.</rdfs:comment>
  </rdfs:Class>
  <rdfs:Class rdf:about="http://e-editiones.ch/ontology/human#AgentGiver">
    <rdfs:comment xml:lang="en">Role of a human agent, person or group, transferring the possession of something to another agent.</rdfs:comment>
    <rdfs:label xml:lang="de">Gebenderrolle eines menschlichen Akteurs</rdfs:label>

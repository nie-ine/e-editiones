  <owl:Ontology rdf:about="http://e-editiones.ch/ontology/text-expression">
    <dct:title xml:lang="en">An ontology about text expression</dct:title>
    <skos:note xml:lang="en">Text, text structure, prosodic structure, note structure, text editing, and scientific editing are described in respective ontologies</skos:note>
    <dct:publisher xml:lang="en">Universities of Basel, Bern, and Zürich, Switzerland</dct:publisher>
    <dct:license rdf:resource="http://creativecommons.org/licenses/by/3.0/"/>
    <dct:creator xml:lang="en">Hans Cools, MD, knowledge engineer, ontologist, software developer, Basel University, Switzerland</dct:creator>
    <dct:contributor xml:lang="en">Roberta Padlina, project coordinator, software developer, Basel University, Switzerland</dct:contributor>
    <dct:description xml:lang="en">Formal description of text expression, based on FRBRoo, in broadest sense, with general subclasses, related classes, and properties.</dct:description>
    <owl:versionInfo rdf:datatype="http://www.w3.org/2001/XMLSchema#date">2020-06-11</owl:versionInfo>
  </owl:Ontology>

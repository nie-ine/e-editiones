  <owl:DatatypeProperty rdf:about="http://e-editiones.ch/ontology/time#hasEarliestStartDateTime">
    <rdfs:isDefinedBy rdf:resource="http://e-editiones.ch/ontology/time#"/>
    <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#dateTime"/>
    <rdfs:domain rdf:resource="http://e-editiones.ch/ontology/event#Event"/>
    <rdfs:label xml:lang="de">hat fr√ºheste Startdatumzeit</rdfs:label>
    <rdfs:comment xml:lang="en">Relating an event to its N3-rule-calculated earliest possible start date time, based on a set maximum event duration, in absence of the start date time.</rdfs:comment>
    <rdfs:label xml:lang="en">has earliest start date time</rdfs:label>
  </owl:DatatypeProperty>
  <owl:ObjectProperty rdf:about="http://e-editiones.ch/ontology/time#calculatingSameTime">
    <rdfs:isDefinedBy rdf:resource="http://e-editiones.ch/ontology/time#"/>
    <rdfs:comment xml:lang="en">Linking 2 lists with each a beginning and end date time typed literal of an event, and events happen during same time.</rdfs:comment>
    <rdfs:label xml:lang="de"></rdfs:label>
    <rdfs:domain rdf:resource="http://www.w3.org/1999/02/22-rdf-syntax-ns#List"/>

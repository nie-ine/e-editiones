    <rdfs:domain rdf:resource="http://www.w3.org/1999/02/22-rdf-syntax-ns#List"/>
    <rdfs:isDefinedBy rdf:resource="http://e-editiones.ch/ontology/time#"/>
    <rdfs:comment xml:lang="en">Linking 2 lists with each a beginning and end date time typed literal of an event, and first event begins after second.</rdfs:comment>
  </owl:ObjectProperty>

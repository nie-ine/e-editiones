    <rdfs:range rdf:resource="http://www.w3.org/1999/02/22-rdf-syntax-ns#List"/>
    <skos:note xml:lang="en">Such a triple invokes in an N3 rule a backward N3 rule checking this time relation for 2 events in premise and returning the triple.</skos:note>
    <rdfs:comment xml:lang="en">Linking 2 lists with each a beginning and end date time typed literal of an event, and first event happens before second.</rdfs:comment>
    <rdfs:domain rdf:resource="http://www.w3.org/1999/02/22-rdf-syntax-ns#List"/>
    <rdfs:isDefinedBy rdf:resource="http://e-editiones.ch/ontology/time#"/>
    <rdfs:label xml:lang="en">calculating before</rdfs:label>
  </owl:ObjectProperty>
  <owl:DatatypeProperty rdf:about="http://e-editiones.ch/ontology/time#hasDate">
    <rdfs:label xml:lang="en">has date</rdfs:label>
    <rdfs:comment xml:lang="en">Relating something to its date.</rdfs:comment>
    <rdfs:label xml:lang="de">hat Datum</rdfs:label>
    <rdfs:domain rdf:resource="http://www.w3.org/2000/01/rdf-schema#Resource"/>
    <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#date"/>
    <rdfs:isDefinedBy rdf:resource="http://e-editiones.ch/ontology/time#"/>
  </owl:DatatypeProperty>
  <owl:DatatypeProperty rdf:about="http://e-editiones.ch/ontology/time#outputHasDateTime">
    <rdfs:label xml:lang="de">Ausgabe hat Datumzeit</rdfs:label>
    <owl:propertyChainAxiom rdf:parseType="Collection">
      <rdf:Description rdf:about="http://e-editiones.ch/ontology/event#isOutputOf"/>
      <rdf:Description rdf:about="http://e-editiones.ch/ontology/time#hasOutputDateTime"/>

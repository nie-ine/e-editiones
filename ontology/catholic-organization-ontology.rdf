<?xml version="1.0" encoding="utf-8" ?>
<rdf:RDF xmlns:rdf="http://www.w3.org/1999/02/22-rdf-syntax-ns#"
         xmlns:owl="http://www.w3.org/2002/07/owl#"
         xmlns:dc="http://purl.org/dc/terms/"
         xmlns:rdfs="http://www.w3.org/2000/01/rdf-schema#">

  <owl:Ontology rdf:about="http://www.e-editiones.ch/ontology/catholic-organization">
    <dc:license rdf:resource="http://creativecommons.org/licenses/by/3.0/"/>
    <dc:title xml:lang="en">An ontology about catholic organization</dc:title>
    <dc:description xml:lang="en">Formal description of catholic organization, general subclasses, related classes, properties, and instances.</dc:description>
    <dc:creator xml:lang="en">Hans Cools, MD, knowledge engineer, ontologist, software developer, Basel University, Switzerland</dc:creator>
    <dc:contributor xml:lang="en">Prof. Dr. Ueli Zahnd, Faculty of Theology, University of Basel, Switzerland</dc:contributor>
    <dc:contributor xml:lang="en">Roberta Padlina, project coordinator and software developer, University of Basel, Switzerland</dc:contributor>
  </owl:Ontology>

  <rdfs:Class rdf:about="http://www.e-editiones.ch/ontology/catholic-organization#Organization">
    <rdfs:isDefinedBy rdf:resource="http://www.e-editiones.ch/ontology/catholic-organization#"/>
    <rdfs:label xml:lang="en">catholic organization</rdfs:label>
    <rdfs:comment xml:lang="en">Organization representing a catholic belief.</rdfs:comment>
    <rdfs:subClassOf rdf:resource="http://www.e-editiones.ch/ontology/organization#ReligiousOrganization"/>
  </rdfs:Class>

  <rdfs:Class rdf:about="http://www.e-editiones.ch/ontology/catholic-organization#Person">
    <rdfs:isDefinedBy rdf:resource="http://www.e-editiones.ch/ontology/catholic-organization#"/>
    <rdfs:label xml:lang="en">catholic person</rdfs:label>
    <rdfs:label xml:lang="de">katholische Person</rdfs:label>
    <rdfs:label xml:lang="fr">personne catholique</rdfs:label>
    <rdfs:label xml:lang="it">persona cattolica</rdfs:label>
    <rdfs:comment xml:lang="en">Person having a catholic belief.</rdfs:comment>
    <rdfs:subClassOf rdf:resource="http://www.e-editiones.ch/ontology/organization#ReligiousPerson"/>
  </rdfs:Class>

  <rdfs:Class rdf:about="http://www.e-editiones.ch/ontology/catholic-organization#SecularClergy">
    <rdfs:isDefinedBy rdf:resource="http://www.e-editiones.ch/ontology/catholic-organization#"/>
    <rdfs:label xml:lang="en">secular clergy</rdfs:label>
    <rdfs:comment xml:lang="en">Catholic organization whose member does not take religious vows and does not live in an institution.</rdfs:comment>
    <rdfs:subClassOf rdf:resource="http://www.e-editiones.ch/ontology/catholic-organization#Organization"/>
  </rdfs:Class>

  <owl:ObjectProperty rdf:about="http://www.e-editiones.ch/ontology/catholic-organization#isMemberOf">
    <rdfs:isDefinedBy rdf:resource="http://www.e-editiones.ch/ontology/catholic-organization#"/>
    <rdfs:label xml:lang="en">is member of catholic organization</rdfs:label>
    <rdfs:label xml:lang="de">ist Mitglied einer katholischen Organization</rdfs:label>
    <rdfs:comment xml:lang="en">Relating a person to a catholic organization the person is a member of.</rdfs:comment>
    <rdfs:domain rdf:resource="http://www.e-editiones.ch/ontology/catholic-organization#Person"/>
    <rdfs:range rdf:resource="http://www.e-editiones.ch/ontology/catholic-organization#Organization"/>
    <rdfs:subPropertyOf rdf:resource="http://www.e-editiones.ch/ontology/organization#isMemberOfReligiousOrganization"/>
  </owl:ObjectProperty>

</rdf:RDF>

<?xml version="1.0" encoding="utf-8"?>
<rdf:RDF
  xmlns:rdfs="http://www.w3.org/2000/01/rdf-schema#"
  xmlns:dct="http://purl.org/dc/terms/"
  xmlns:owl="http://www.w3.org/2002/07/owl#"
  xmlns:rdf="http://www.w3.org/1999/02/22-rdf-syntax-ns#"
>
  <owl:ObjectProperty rdf:about="http://e-editiones.ch/ontology/letter-corresponding#isStructureOfExpression">
    <rdfs:subPropertyOf rdf:resource="http://e-editiones.ch/ontology/text-structure#isPartOfTextExpression"/>
    <rdfs:isDefinedBy rdf:resource="http://e-editiones.ch/ontology/letter-corresponding#"/>
    <rdfs:range rdf:resource="http://e-editiones.ch/ontology/letter-corresponding#Expression"/>
    <rdfs:label xml:lang="fr">est la structure de lettre d'une expression</rdfs:label>
    <rdfs:domain rdf:resource="http://e-editiones.ch/ontology/letter-corresponding#Structure"/>
    <rdfs:label xml:lang="de">ist Briefstruktur einer Expression</rdfs:label>
    <rdfs:label xml:lang="it">è la struttura delle lettere di un'espressione</rdfs:label>
    <rdfs:comment xml:lang="en">Relating a letter structure to a text expression it is part of.</rdfs:comment>
    <rdfs:label xml:lang="en">is letter structure of expression</rdfs:label>
  </owl:ObjectProperty>
  <owl:ObjectProperty rdf:about="http://e-editiones.ch/ontology/letter-corresponding#isExchangingOf">
    <rdfs:label xml:lang="en">is exchanging of</rdfs:label>
    <rdfs:range rdf:resource="http://e-editiones.ch/ontology/letter-corresponding#Corresponding"/>
    <rdfs:label xml:lang="fr">est un échange de</rdfs:label>
    <rdfs:comment xml:lang="en">Relating an exchanging to its corresponding it is a suprocedure of.</rdfs:comment>
    <rdfs:subPropertyOf rdf:resource="http://e-editiones.ch/ontology/event#isSubProcedureOf"/>
    <rdfs:label xml:lang="de">ist Austausch von</rdfs:label>
    <rdfs:isDefinedBy rdf:resource="http://e-editiones.ch/ontology/letter-corresponding#"/>
    <rdfs:domain rdf:resource="http://e-editiones.ch/ontology/letter-corresponding#Exchanging"/>
    <rdfs:label xml:lang="it">è uno scambio di</rdfs:label>
  </owl:ObjectProperty>
  <owl:ObjectProperty rdf:about="http://e-editiones.ch/ontology/letter-corresponding#isInLetter">
    <rdfs:comment xml:lang="en">Relating a letter expression to a letter it is in.</rdfs:comment>
    <rdfs:label xml:lang="de">ist in Brief</rdfs:label>
    <rdfs:label xml:lang="fr">est en lettre</rdfs:label>
    <rdfs:isDefinedBy rdf:resource="http://e-editiones.ch/ontology/letter-corresponding#"/>
    <rdfs:domain rdf:resource="http://e-editiones.ch/ontology/letter-corresponding#Expression"/>
    <rdfs:range rdf:resource="http://e-editiones.ch/ontology/letter-corresponding#Letter"/>
    <rdfs:subPropertyOf rdf:resource="http://e-editiones.ch/ontology/information-carrier#isOnCarrier"/>
    <rdfs:label xml:lang="it">è nella lettera</rdfs:label>
    <rdfs:label xml:lang="en">is in letter</rdfs:label>
  </owl:ObjectProperty>
  <owl:ObjectProperty rdf:about="http://e-editiones.ch/ontology/letter-corresponding#isPageOfLetter">
    <rdfs:comment xml:lang="en">Relating a page to the letter it is part of.</rdfs:comment>
    <rdfs:label xml:lang="de">ist Seite eines Briefes</rdfs:label>
    <rdfs:domain rdf:resource="http://e-editiones.ch/ontology/information-carrier#Page"/>
    <rdfs:label xml:lang="en">is page of letter</rdfs:label>
    <rdfs:label xml:lang="fr">est une page d'une lettre</rdfs:label>
    <rdfs:label xml:lang="it">è una pagina di una lettera</rdfs:label>
    <rdfs:isDefinedBy rdf:resource="http://e-editiones.ch/ontology/letter-corresponding#"/>
    <rdfs:subPropertyOf rdf:resource="http://e-editiones.ch/ontology/physical-entity#isPartOf"/>
    <rdfs:range rdf:resource="http://e-editiones.ch/ontology/letter-corresponding#Letter"/>
  </owl:ObjectProperty>
  <owl:ObjectProperty rdf:about="http://e-editiones.ch/ontology/letter-corresponding#hasEnvelope">
    <rdfs:label xml:lang="en">letter has envelope</rdfs:label>
    <rdfs:isDefinedBy rdf:resource="http://e-editiones.ch/ontology/letter-corresponding#"/>
    <rdfs:label xml:lang="fr">lettre a une enveloppe</rdfs:label>
    <rdfs:comment xml:lang="en">Relating a letter to an envelope it has.</rdfs:comment>
    <rdfs:range rdf:resource="http://e-editiones.ch/ontology/letter-corresponding#Envelope"/>
    <rdfs:domain rdf:resource="http://e-editiones.ch/ontology/letter-corresponding#Letter"/>
    <rdfs:label xml:lang="de">Brief hat Umschlag</rdfs:label>
    <rdfs:label xml:lang="it">lettera ha una busta</rdfs:label>
  </owl:ObjectProperty>
  <owl:Ontology rdf:about="http://e-editiones.ch/ontology/letter-corresponding">
    <dct:publisher xml:lang="en">Universities of Basel, Bern, and Zürich, Switzerland</dct:publisher>
    <owl:versionInfo rdf:datatype="http://www.w3.org/2001/XMLSchema#date">2020-06-11</owl:versionInfo>
    <dct:contributor xml:lang="en">Roberta Padlina, project coordinator and software developer, University of Basel, Switzerland</dct:contributor>
    <dct:contributor xml:lang="en">Reto Baumgartner, tool coordinator and software developer, University of Zürich, Switzerland</dct:contributor>
    <dct:description xml:lang="en">Formal description of letter corresponding, related classes, and properties.</dct:description>
    <dct:license rdf:resource="http://creativecommons.org/licenses/by/3.0/"/>
    <dct:title xml:lang="en">An ontology about letter corresponding</dct:title>
    <dct:creator xml:lang="en">Hans Cools, MD, knowledge engineer, ontologist, software developer, University of Basel, Switzerland</dct:creator>
  </owl:Ontology>
  <owl:ObjectProperty rdf:about="http://e-editiones.ch/ontology/letter-corresponding#hasEnclosure">
    <rdfs:label xml:lang="it">lettera ha un allegato</rdfs:label>
    <rdfs:label xml:lang="fr">lettre a une pièce jointe</rdfs:label>
    <rdfs:domain rdf:resource="http://e-editiones.ch/ontology/letter-corresponding#Letter"/>
    <rdfs:isDefinedBy rdf:resource="http://e-editiones.ch/ontology/letter-corresponding#"/>
    <rdfs:label xml:lang="de">Brief hat Beilage</rdfs:label>
    <rdfs:label xml:lang="en">letter has enclosure</rdfs:label>
    <rdfs:comment xml:lang="en">Relating a letter to an enclosure it has.</rdfs:comment>
    <rdfs:range rdf:resource="http://e-editiones.ch/ontology/letter-corresponding#Enclosure"/>
  </owl:ObjectProperty>
  <owl:ObjectProperty rdf:about="http://e-editiones.ch/ontology/letter-corresponding#isTextInEnclosure">
    <rdfs:label xml:lang="de">ist Textexpression in Briefbeilage</rdfs:label>
    <rdfs:range rdf:resource="http://e-editiones.ch/ontology/letter-corresponding#InformationEnclosure"/>
    <rdfs:subPropertyOf rdf:resource="http://e-editiones.ch/ontology/information-carrier#isOnCarrier"/>
    <rdfs:label xml:lang="en">is text expression in letter enclosure</rdfs:label>
    <rdfs:label xml:lang="it">è un'espressione di testo in un allegato di lettera</rdfs:label>
    <rdfs:label xml:lang="fr">est une expression textuelle dans une pièce-jointe à une lettre</rdfs:label>
    <rdfs:domain rdf:resource="http://e-editiones.ch/ontology/letter-corresponding#EnclosureExpression"/>
    <rdfs:comment xml:lang="en">Relating a text expression to a letter information enclosure it is in.</rdfs:comment>
    <rdfs:isDefinedBy rdf:resource="http://e-editiones.ch/ontology/letter-corresponding#"/>
  </owl:ObjectProperty>
  <owl:ObjectProperty rdf:about="http://e-editiones.ch/ontology/letter-corresponding#hasEnvelopeDescription">
    <rdfs:range rdf:resource="http://www.w3.org/2000/01/rdf-schema#Literal"/>
    <rdfs:label xml:lang="fr">lettre a une description de l'enveloppe</rdfs:label>
    <rdfs:comment xml:lang="en">Relating a letter to a description of its envelope.</rdfs:comment>
    <rdfs:domain rdf:resource="http://e-editiones.ch/ontology/letter-corresponding#Letter"/>
    <rdfs:isDefinedBy rdf:resource="http://e-editiones.ch/ontology/letter-corresponding#"/>
    <rdfs:label xml:lang="de">Brief hat Umschlagbeschreibung</rdfs:label>
    <rdfs:label xml:lang="it">lettera ha una descrizione della busta</rdfs:label>
    <rdfs:label xml:lang="en">letter has envelope description</rdfs:label>
  </owl:ObjectProperty>
  <owl:ObjectProperty rdf:about="http://e-editiones.ch/ontology/letter-corresponding#isCompilingOf">
    <rdfs:range rdf:resource="http://e-editiones.ch/ontology/letter-corresponding#Exchanging"/>
    <rdfs:label xml:lang="fr">est la compilation de</rdfs:label>
    <rdfs:label xml:lang="de">ist Zusammenstellung von</rdfs:label>
    <rdfs:isDefinedBy rdf:resource="http://e-editiones.ch/ontology/letter-corresponding#"/>
    <rdfs:domain rdf:resource="http://e-editiones.ch/ontology/letter-corresponding#Compiling"/>
    <rdfs:subPropertyOf rdf:resource="http://e-editiones.ch/ontology/event#isSubProcedureOf"/>
    <rdfs:label xml:lang="it">è la compilazione di</rdfs:label>
    <rdfs:label xml:lang="en">is compiling of</rdfs:label>
    <rdfs:comment xml:lang="en">Relating a compiling to its exchanging it is a suprocedure of.</rdfs:comment>
  </owl:ObjectProperty>
  <owl:ObjectProperty rdf:about="http://e-editiones.ch/ontology/letter-corresponding#follows">
    <rdfs:isDefinedBy rdf:resource="http://e-editiones.ch/ontology/letter-corresponding#"/>
    <rdfs:range rdf:resource="http://e-editiones.ch/ontology/letter-corresponding#Letter"/>
    <rdfs:label xml:lang="it">lettera successiva a</rdfs:label>
    <rdfs:label xml:lang="en">letter follows</rdfs:label>
    <rdfs:label xml:lang="fr">lettre suivant de</rdfs:label>
    <rdfs:comment xml:lang="en">Relating a letter to a preceding letter.</rdfs:comment>
    <rdfs:domain rdf:resource="http://e-editiones.ch/ontology/letter-corresponding#Letter"/>
    <rdfs:label xml:lang="de">Brief folgt</rdfs:label>
  </owl:ObjectProperty>
  <rdfs:Class rdf:about="http://e-editiones.ch/ontology/letter-corresponding#Exchanging">
    <rdfs:label xml:lang="de">Austausch</rdfs:label>
    <rdfs:label xml:lang="it">scambiare</rdfs:label>
    <rdfs:label xml:lang="en">exchanging</rdfs:label>
    <rdfs:comment xml:lang="en">Sending-receiving cycle in corresponding.</rdfs:comment>
    <rdfs:label xml:lang="fr">échanger</rdfs:label>
    <rdfs:isDefinedBy rdf:resource="http://e-editiones.ch/ontology/letter-corresponding#"/>
    <rdfs:subClassOf rdf:resource="http://e-editiones.ch/ontology/human#Procedure"/>
  </rdfs:Class>
  <rdfs:Class rdf:about="http://e-editiones.ch/ontology/letter-corresponding#Structure">
    <rdfs:label xml:lang="de">Briefstruktur</rdfs:label>
    <rdfs:isDefinedBy rdf:resource="http://e-editiones.ch/ontology/letter-corresponding#"/>
    <rdfs:label xml:lang="en">letter structure</rdfs:label>
    <rdfs:subClassOf rdf:resource="http://e-editiones.ch/ontology/text-structure#CompositionalContentStructure"/>
    <rdfs:comment xml:lang="en">Text with letter structure.</rdfs:comment>
    <rdfs:label xml:lang="it">struttura della lettera</rdfs:label>
    <rdfs:label xml:lang="fr">structure de lettre</rdfs:label>
  </rdfs:Class>
  <rdfs:Class rdf:about="http://e-editiones.ch/ontology/letter-corresponding#Enclosure">
    <rdfs:isDefinedBy rdf:resource="http://e-editiones.ch/ontology/letter-corresponding#"/>
    <rdfs:label xml:lang="it">allegato di una lettera</rdfs:label>
    <rdfs:subClassOf rdf:resource="http://e-editiones.ch/ontology/creating#PhysicalCreation"/>
    <rdfs:label xml:lang="fr">pièce jointe d'une lettre</rdfs:label>
    <rdfs:comment xml:lang="en">Something added to a letter being sent, e.g. another information carrier.</rdfs:comment>
    <rdfs:label xml:lang="en">letter enclosure</rdfs:label>
    <rdfs:label xml:lang="de">Briefbeilage</rdfs:label>
  </rdfs:Class>
  <rdfs:Class rdf:about="http://e-editiones.ch/ontology/letter-corresponding#Corresponding">
    <rdfs:subClassOf rdf:resource="http://e-editiones.ch/ontology/human#Procedure"/>
    <rdfs:label xml:lang="de">Korrespondieren</rdfs:label>
    <rdfs:isDefinedBy rdf:resource="http://e-editiones.ch/ontology/letter-corresponding#"/>
    <rdfs:comment xml:lang="en">Communicating by exchanging letters.</rdfs:comment>
    <rdfs:label xml:lang="en">corresponding</rdfs:label>
    <rdfs:label xml:lang="fr">correspondre</rdfs:label>
    <rdfs:label xml:lang="it">corrispondere</rdfs:label>
  </rdfs:Class>
  <rdfs:Class rdf:about="http://e-editiones.ch/ontology/letter-corresponding#InformationEnclosure">
    <rdfs:label xml:lang="fr">information annexe à une lettre</rdfs:label>
    <rdfs:comment xml:lang="en">Information carrier as letter enclosure.</rdfs:comment>
    <rdfs:subClassOf rdf:resource="http://e-editiones.ch/ontology/letter-corresponding#Enclosure"/>
    <rdfs:label xml:lang="it">allegato lettera informativa</rdfs:label>
    <rdfs:label xml:lang="de">Information-Briefbeilage</rdfs:label>
    <rdfs:label xml:lang="en">information letter enclosure</rdfs:label>
    <rdfs:isDefinedBy rdf:resource="http://e-editiones.ch/ontology/letter-corresponding#"/>
    <rdfs:subClassOf rdf:resource="http://e-editiones.ch/ontology/information-carrier#Carrier"/>
  </rdfs:Class>
  <rdfs:Class rdf:about="http://e-editiones.ch/ontology/letter-corresponding#Envelope">
    <rdfs:label xml:lang="fr">enveloppe</rdfs:label>
    <rdfs:isDefinedBy rdf:resource="http://e-editiones.ch/ontology/letter-corresponding#"/>
    <rdfs:comment xml:lang="en">Flat paper container with a sealable flap for sending something, e.g. a letter, from (a) person(s) or (an) Organization(s) to another one, e.g. through a postal system.</rdfs:comment>
    <rdfs:label xml:lang="it">busta</rdfs:label>
    <rdfs:label xml:lang="en">envelope</rdfs:label>
    <rdfs:subClassOf rdf:resource="http://e-editiones.ch/ontology/creating#PhysicalCreation"/>
    <rdfs:label xml:lang="de">Umschlag</rdfs:label>
  </rdfs:Class>
  <rdfs:Class rdf:about="http://e-editiones.ch/ontology/letter-corresponding#Letter">
    <rdfs:comment xml:lang="en">Written text, e.g. on a leaf, intended to be sent from (a) person(s) or (an) Organization(s) to (an)other one(s), e.g. through a postal system.</rdfs:comment>
    <rdfs:label xml:lang="it">lettera</rdfs:label>
    <rdfs:label xml:lang="en">letter</rdfs:label>
    <rdfs:subClassOf rdf:resource="http://e-editiones.ch/ontology/information-carrier#Carrier"/>
    <rdfs:isDefinedBy rdf:resource="http://e-editiones.ch/ontology/letter-corresponding#"/>
    <rdfs:label xml:lang="de">Brief</rdfs:label>
    <rdfs:subClassOf rdf:resource="http://iflastandards.info/ns/fr/frbr/frbroo/F4_Manifestation_Singleton"/>
    <rdfs:label xml:lang="fr">lettre</rdfs:label>
  </rdfs:Class>
  <rdfs:Class rdf:about="http://e-editiones.ch/ontology/letter-corresponding#Compiling">
    <rdfs:subClassOf rdf:resource="http://e-editiones.ch/ontology/human#Procedure"/>
    <rdfs:comment xml:lang="en">Preparing a letter for sending, mostly by putting it in an envelope, possibly adding an enclosure.</rdfs:comment>
    <rdfs:label xml:lang="en">compiling</rdfs:label>
    <rdfs:isDefinedBy rdf:resource="http://e-editiones.ch/ontology/letter-corresponding#"/>
    <rdfs:label xml:lang="fr">compiler</rdfs:label>
    <rdfs:label xml:lang="de">Zusammenstellen</rdfs:label>
    <rdfs:label xml:lang="it">compilare</rdfs:label>
  </rdfs:Class>
  <rdfs:Class rdf:about="http://e-editiones.ch/ontology/letter-corresponding#EnclosureExpression">
    <rdfs:label xml:lang="de">Briefbeilagen-Textexpression</rdfs:label>
    <rdfs:label xml:lang="it">espressione di testo allegato lettera</rdfs:label>
    <rdfs:subClassOf rdf:resource="http://e-editiones.ch/ontology/text-expression#IndividualExpression"/>
    <rdfs:label xml:lang="fr">expression de texte d'une pièce jointe d'une lettre</rdfs:label>
    <rdfs:isDefinedBy rdf:resource="http://e-editiones.ch/ontology/letter-corresponding#"/>
    <rdfs:label xml:lang="en">letter enclosure text expression</rdfs:label>
    <rdfs:comment xml:lang="en">Text expression of a letter enclosure.</rdfs:comment>
  </rdfs:Class>
  <rdfs:Class rdf:about="http://e-editiones.ch/ontology/letter-corresponding#Expression">
    <rdfs:label xml:lang="it">espressione letterale</rdfs:label>
    <rdfs:label xml:lang="en">letter expression</rdfs:label>
    <rdfs:subClassOf rdf:resource="http://e-editiones.ch/ontology/text-expression#IndividualExpression"/>
    <rdfs:label xml:lang="de">Briefexpression</rdfs:label>
    <rdfs:isDefinedBy rdf:resource="http://e-editiones.ch/ontology/letter-corresponding#"/>
    <rdfs:comment xml:lang="en">Text expression with a letter content and possibly structure.</rdfs:comment>
    <rdfs:label xml:lang="fr">expression de lettre</rdfs:label>
  </rdfs:Class>
</rdf:RDF>

<?xml version="1.0" encoding="utf-8"?>
<rdf:RDF
  xmlns:rdfs="http://www.w3.org/2000/01/rdf-schema#"
  xmlns:dct="http://purl.org/dc/terms/"
  xmlns:owl="http://www.w3.org/2002/07/owl#"
  xmlns:rdf="http://www.w3.org/1999/02/22-rdf-syntax-ns#"
>
  <rdfs:Class rdf:about="http://www.e-editiones.ch/ontology/device#MechanicalMachine">
    <rdfs:isDefinedBy rdf:resource="http://www.e-editiones.ch/ontology/device#"/>
    <rdfs:label xml:lang="en">mechanical machine</rdfs:label>
    <rdfs:comment xml:lang="en">Machine using kinetic energy to perform a task.</rdfs:comment>
    <rdfs:subClassOf rdf:resource="http://www.e-editiones.ch/ontology/device#Machine"/>
    <rdfs:label xml:lang="de">mechanische Maschine</rdfs:label>
  </rdfs:Class>
  <rdfs:Class rdf:about="http://www.e-editiones.ch/ontology/device#DigitalElectronicComputer">
    <rdfs:label xml:lang="de">digitale elektronische Rechner</rdfs:label>
    <rdfs:isDefinedBy rdf:resource="http://www.e-editiones.ch/ontology/device#"/>
    <rdfs:comment xml:lang="en">Electronic computer using digital software and data to perform a task.</rdfs:comment>
    <rdfs:subClassOf rdf:resource="http://www.e-editiones.ch/ontology/device#DigitalComputer"/>
    <rdfs:subClassOf rdf:resource="http://www.e-editiones.ch/ontology/device#ElectronicComputer"/>
    <rdfs:subClassOf rdf:resource="http://www.e-editiones.ch/ontology/information-carrier#ElectronicCarrier"/>
    <rdfs:label xml:lang="en">digital electronic computer</rdfs:label>
  </rdfs:Class>
  <owl:Ontology rdf:about="http://www.e-editiones.ch/ontology/device">
    <dct:license rdf:resource="http://creativecommons.org/licenses/by/3.0/"/>
    <dct:description xml:lang="en">Formal description of a device, general subclasses, related classes and properties.</dct:description>
    <dct:title xml:lang="en">An ontology about device</dct:title>
    <dct:publisher xml:lang="en">Universities of Basel, Bern, and Zürich, Switzerland</dct:publisher>
    <dct:creator xml:lang="en">Hans Cools, MD, knowledge engineer, ontologist, software developer, Basel University, Switzerland</dct:creator>
  </owl:Ontology>
  <rdfs:Class rdf:about="http://www.e-editiones.ch/ontology/device#Machine">
    <rdfs:label xml:lang="de">Maschine</rdfs:label>
    <rdfs:label xml:lang="en">machine</rdfs:label>
    <rdfs:comment xml:lang="en">Device having more than one part, each with a function, and transmitting a force or directing its application to perform a task.</rdfs:comment>
    <rdfs:isDefinedBy rdf:resource="http://www.e-editiones.ch/ontology/device#"/>
    <rdfs:subClassOf rdf:resource="http://www.e-editiones.ch/ontology/device#Device"/>
    <rdfs:subClassOf rdf:resource="http://www.e-editiones.ch/ontology/agent#Agent"/>
  </rdfs:Class>
  <rdfs:Class rdf:about="http://www.e-editiones.ch/ontology/device#ElectronicMachine">
    <rdfs:isDefinedBy rdf:resource="http://www.e-editiones.ch/ontology/device#"/>
    <rdfs:comment xml:lang="en">Electric machine manipulating the motion of electrons in an electrical current in ways that add meaningful information to the current, to perform a task.</rdfs:comment>
    <rdfs:label xml:lang="de">elektronische Maschine</rdfs:label>
    <rdfs:label xml:lang="en">electronic machine</rdfs:label>
    <rdfs:subClassOf rdf:resource="http://www.e-editiones.ch/ontology/device#ElectricMachine"/>
  </rdfs:Class>
  <rdfs:Class rdf:about="http://www.e-editiones.ch/ontology/device#ElectronicComputer">
    <rdfs:label xml:lang="de">elektronische Rechner</rdfs:label>
    <rdfs:subClassOf rdf:resource="http://www.e-editiones.ch/ontology/device#Computer"/>
    <rdfs:isDefinedBy rdf:resource="http://www.e-editiones.ch/ontology/device#"/>
    <rdfs:comment xml:lang="en">Computer also being an electronic machine.</rdfs:comment>
    <rdfs:label xml:lang="en">electronic computer</rdfs:label>
    <rdfs:subClassOf rdf:resource="http://www.e-editiones.ch/ontology/device#ElectronicMachine"/>
  </rdfs:Class>
  <rdfs:Class rdf:about="http://www.e-editiones.ch/ontology/device#DigitalComputer">
    <rdfs:comment xml:lang="en">Computer ....</rdfs:comment>
    <rdfs:isDefinedBy rdf:resource="http://www.e-editiones.ch/ontology/device#"/>
    <rdfs:subClassOf rdf:resource="http://www.e-editiones.ch/ontology/device#Computer"/>
    <rdfs:label xml:lang="de">digitale Rechner</rdfs:label>
    <rdfs:label xml:lang="en">digital computer</rdfs:label>
  </rdfs:Class>
  <rdfs:Class rdf:about="http://www.e-editiones.ch/ontology/device#ElectricMachine">
    <rdfs:label xml:lang="de">elektrische Maschine</rdfs:label>
    <rdfs:isDefinedBy rdf:resource="http://www.e-editiones.ch/ontology/device#"/>
    <rdfs:label xml:lang="en">electric machine</rdfs:label>
    <rdfs:subClassOf rdf:resource="http://www.e-editiones.ch/ontology/device#Machine"/>
    <rdfs:comment xml:lang="en">Machine using electric potential energy to convert it via electrical current to kinetic energy to perform a task.</rdfs:comment>
  </rdfs:Class>
  <rdfs:Class rdf:about="http://www.e-editiones.ch/ontology/device#Computer">
    <rdfs:subClassOf rdf:resource="http://www.e-editiones.ch/ontology/device#Machine"/>
    <rdfs:isDefinedBy rdf:resource="http://www.e-editiones.ch/ontology/device#"/>
    <rdfs:label xml:lang="de">Rechner</rdfs:label>
    <rdfs:comment xml:lang="en">Machine using programmed instructions to perform arithmetic and logic operations automatically, to perform a task.</rdfs:comment>
    <rdfs:label xml:lang="en">computer</rdfs:label>
  </rdfs:Class>
  <rdfs:Class rdf:about="http://www.e-editiones.ch/ontology/device#Device">
    <rdfs:comment xml:lang="en">Physical creation serving a particular purpose.</rdfs:comment>
    <rdfs:subClassOf rdf:resource="http://www.e-editiones.ch/ontology/creating#PhysicalCreation"/>
    <rdfs:label xml:lang="en">device</rdfs:label>
    <rdfs:isDefinedBy rdf:resource="http://www.e-editiones.ch/ontology/device#"/>
    <rdfs:label xml:lang="de">Gerät</rdfs:label>
  </rdfs:Class>
</rdf:RDF>

<?xml version="1.0" encoding="utf-8"?>
<rdf:RDF
  xmlns:rdfs="http://www.w3.org/2000/01/rdf-schema#"
  xmlns:dct="http://purl.org/dc/terms/"
  xmlns:owl="http://www.w3.org/2002/07/owl#"
  xmlns:rdf="http://www.w3.org/1999/02/22-rdf-syntax-ns#"
>
  <owl:Ontology rdf:about="http://www.e-editiones.ch/ontology/philosophies">
    <dct:license rdf:resource="http://creativecommons.org/licenses/by/3.0/"/>
    <dct:contributor xml:lang="en">Prof. Dr. Ueli Zahnd, Faculty of Theology, University of Basel, Switzerland</dct:contributor>
    <dct:creator xml:lang="en">Hans Cools, MD, knowledge engineer, ontologist, software developer, Basel University, Switzerland</dct:creator>
    <dct:contributor xml:lang="en">Gabriel Müller, Faculty of Theology, University of Basel, Switzerland</dct:contributor>
    <dct:title xml:lang="en">An ontology about philosophies</dct:title>
    <dct:description xml:lang="en">Formal description of philosophies, general subclasses, related classes, properties, and instances.</dct:description>
  </owl:Ontology>
  <rdfs:Class rdf:about="http://www.e-editiones.ch/ontology/philosophies#Albertism">
    <rdfs:subClassOf rdf:resource="http://www.e-editiones.ch/ontology/philosophy#Philosophy"/>
    <rdfs:comment xml:lang="en">Philosophy and theology based on the ideas of Albertus Magnus (before 1200 - 1280-11-15).</rdfs:comment>
    <rdfs:isDefinedBy rdf:resource="http://www.e-editiones.ch/ontology/philosophies#"/>
    <rdfs:label xml:lang="en">Albertism</rdfs:label>
  </rdfs:Class>
  <rdfs:Class rdf:about="http://www.e-editiones.ch/ontology/philosophies#Thomism">
    <rdfs:subClassOf rdf:resource="http://www.e-editiones.ch/ontology/philosophy#Philosophy"/>
    <rdfs:isDefinedBy rdf:resource="http://www.e-editiones.ch/ontology/philosophies#"/>
    <rdfs:comment xml:lang="en">Philosophy based on the work of Thomas Aquinas (1225 - 1274-O3-O7).</rdfs:comment>
    <rdfs:label xml:lang="en">Thomism</rdfs:label>
  </rdfs:Class>
  <rdfs:Class rdf:about="http://www.e-editiones.ch/ontology/philosophies#Nominalism">
    <rdfs:comment xml:lang="en">Philosophy stating that only individual things exist, denying the existence of universals and abstract objects.</rdfs:comment>
    <rdfs:isDefinedBy rdf:resource="http://www.e-editiones.ch/ontology/philosophies#"/>
    <rdfs:subClassOf rdf:resource="http://www.e-editiones.ch/ontology/philosophy#Metaphysics"/>
    <rdfs:label xml:lang="en">Nominalism</rdfs:label>
  </rdfs:Class>
  <owl:ObjectProperty rdf:about="http://www.e-editiones.ch/ontology/philosophies#adheringToThought">
    <rdfs:comment xml:lang="en">Relating a person to a philosophy the person adheres to.</rdfs:comment>
    <rdfs:domain rdf:resource="http://www.e-editiones.ch/ontology/human#Person"/>
    <rdfs:subPropertyOf rdf:resource="http://www.e-editiones.ch/ontology/human#adheringToThought"/>
    <rdfs:range rdf:resource="http://www.e-editiones.ch/ontology/philosophy#Philosophy"/>
    <rdfs:label xml:lang="en">adhering to philosophy</rdfs:label>
    <rdfs:isDefinedBy rdf:resource="http://www.e-editiones.ch/ontology/philosophies#"/>
    <rdfs:label xml:lang="de">ist Anhänger von Philosophie</rdfs:label>
  </owl:ObjectProperty>
  <rdfs:Class rdf:about="http://www.e-editiones.ch/ontology/philosophies#Humanism">
    <rdfs:subClassOf rdf:resource="http://www.e-editiones.ch/ontology/philosophy#Philosophy"/>
    <rdfs:isDefinedBy rdf:resource="http://www.e-editiones.ch/ontology/philosophies#"/>
    <rdfs:label xml:lang="en">Humanism</rdfs:label>
    <rdfs:comment xml:lang="en">.</rdfs:comment>
  </rdfs:Class>
  <rdfs:Class rdf:about="http://www.e-editiones.ch/ontology/philosophies#Scotism">
    <rdfs:comment xml:lang="en">.</rdfs:comment>
    <rdfs:subClassOf rdf:resource="http://www.e-editiones.ch/ontology/philosophy#Philosophy"/>
    <rdfs:isDefinedBy rdf:resource="http://www.e-editiones.ch/ontology/philosophies#"/>
    <rdfs:label xml:lang="en">Scotism</rdfs:label>
  </rdfs:Class>
</rdf:RDF>

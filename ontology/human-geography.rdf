<?xml version="1.0" encoding="utf-8" ?>
<rdf:RDF xmlns:rdf="http://www.w3.org/1999/02/22-rdf-syntax-ns#"
         xmlns:owl="http://www.w3.org/2002/07/owl#"
         xmlns:dc="http://purl.org/dc/terms/"
         xmlns:rdfs="http://www.w3.org/2000/01/rdf-schema#">

  <owl:Ontology rdf:about="http://www.e-editiones.ch/ontology/human-geography">
    <dc:license rdf:resource="http://creativecommons.org/licenses/by/3.0/"/>
    <dc:title xml:lang="en">Human Geography</dc:title>
    <dc:description xml:lang="en">Formal description of human geography, general subclasses, related classes, and properties.</dc:description>
    <dc:creator xml:lang="en">Hans Cools, MD, knowledge engineer, ontologist, software developer, NIE-INE, Basel University, Switzerland</dc:creator>
    <dc:publisher xml:lang="en">Universities of Basel, Bern, and Z체rich, Switzerland</dc:publisher>
  </owl:Ontology>

  <rdfs:Class rdf:about="http://www.e-editiones.ch/ontology/human-geography#Nation">
    <rdfs:isDefinedBy rdf:resource="http://www.e-editiones.ch/ontology/human-geography#"/>
    <rdfs:label xml:lang="en">nation</rdfs:label>
    <rdfs:label xml:lang="de">Nation</rdfs:label>
    <rdfs:comment xml:lang="en">Stable political organization with a commonality e.g. ethnicity, language, culture, .</rdfs:comment>
    <rdfs:subClassOf rdf:resource="http://www.e-editiones.ch/ontology/organization#PoliticalOrganization"/>
  </rdfs:Class>

  <rdfs:Class rdf:about="http://www.e-editiones.ch/ontology/human-geography#Country">
    <rdfs:isDefinedBy rdf:resource="http://www.e-editiones.ch/ontology/human-geography#"/>
    <rdfs:label xml:lang="en">country</rdfs:label>
    <rdfs:label xml:lang="de">Land</rdfs:label>
    <rdfs:comment xml:lang="en">Place of a nation.</rdfs:comment>
    <rdfs:subClassOf rdf:resource="http://www.e-editiones.ch/ontology/physical-entity#EarthPlace"/>
  </rdfs:Class>

  <rdfs:Class rdf:about="http://www.e-editiones.ch/ontology/human-geography#State">
    <rdfs:isDefinedBy rdf:resource="http://www.e-editiones.ch/ontology/human-geography#"/>
    <rdfs:label xml:lang="en">state</rdfs:label>
    <rdfs:label xml:lang="de">Staat</rdfs:label>
    <rdfs:comment xml:lang="en">Political organization of a nation with an own government.</rdfs:comment>
    <rdfs:subClassOf rdf:resource="http://www.e-editiones.ch/ontology/organization#PoliticalOrganization"/>
  </rdfs:Class>

  <rdfs:Class rdf:about="http://www.e-editiones.ch/ontology/human-geography#Monarchy">
    <rdfs:isDefinedBy rdf:resource="http://www.e-editiones.ch/ontology/human-geography#"/>
    <rdfs:label xml:lang="en">monarchy</rdfs:label>
    <rdfs:label xml:lang="de">Monarchie</rdfs:label>
    <rdfs:comment xml:lang="en">State wherein one person or family governs.</rdfs:comment>
    <rdfs:subClassOf rdf:resource="http://www.e-editiones.ch/ontology/human-geography#State"/>
  </rdfs:Class>

  <rdfs:Class rdf:about="http://www.e-editiones.ch/ontology/human-geography#Monarch">
    <rdfs:isDefinedBy rdf:resource="http://www.e-editiones.ch/ontology/human-geography#"/>
    <rdfs:label xml:lang="en">monarch</rdfs:label>
    <rdfs:label xml:lang="de">Monarch</rdfs:label>
    <rdfs:label xml:lang="fr">monarque</rdfs:label>
    <rdfs:comment xml:lang="en">Role of a person being a sovereign head of state in a monarchy.</rdfs:comment>
    <rdfs:subClassOf rdf:resource="http://www.e-editiones.ch/ontology/human#PersonRole"/>
  </rdfs:Class>

  <rdfs:Class rdf:about="http://www.e-editiones.ch/ontology/human-geography#Empire">
    <rdfs:isDefinedBy rdf:resource="http://www.e-editiones.ch/ontology/human-geography#"/>
    <rdfs:label xml:lang="en">empire</rdfs:label>
    <rdfs:label xml:lang="de">Imperium</rdfs:label>
    <rdfs:comment xml:lang="en">Group of nations with an overarching government.</rdfs:comment>
    <rdfs:subClassOf rdf:resource="http://www.e-editiones.ch/ontology/organization#PoliticalOrganization"/>
  </rdfs:Class>

  <rdfs:Class rdf:about="http://www.e-editiones.ch/ontology/human-geography#Town">
    <rdfs:isDefinedBy rdf:resource="http://www.e-editiones.ch/ontology/human-geography#"/>
    <rdfs:label xml:lang="en">town</rdfs:label>
    <rdfs:label xml:lang="de">Ortschaft</rdfs:label>
    <rdfs:comment xml:lang="en">Smallest place with political boundaries and an own government.</rdfs:comment>
    <rdfs:subClassOf rdf:resource="http://www.e-editiones.ch/ontology/physical-entity#EarthPlace"/>
  </rdfs:Class>

  <rdfs:Class rdf:about="http://www.e-editiones.ch/ontology/human-geography#Village">
    <rdfs:isDefinedBy rdf:resource="http://www.e-editiones.ch/ontology/human-geography#"/>
    <rdfs:label xml:lang="en">village</rdfs:label>
    <rdfs:label xml:lang="de">Dorf</rdfs:label>
    <rdfs:comment xml:lang="en">Small place with political boundaries but without an own government.</rdfs:comment>
    <rdfs:subClassOf rdf:resource="http://www.e-editiones.ch/ontology/physical-entity#EarthPlace"/>
  </rdfs:Class>

  <rdfs:Class rdf:about="http://www.e-editiones.ch/ontology/human-geography#City">
    <rdfs:isDefinedBy rdf:resource="http://www.e-editiones.ch/ontology/human-geography#"/>
    <rdfs:label xml:lang="en">city</rdfs:label>
    <rdfs:label xml:lang="de">Stadt</rdfs:label>
    <rdfs:comment xml:lang="en">Large town.</rdfs:comment>
    <rdfs:subClassOf rdf:resource="http://www.e-editiones.ch/ontology/physical-entity#EarthPlace"/>
  </rdfs:Class>

  <rdfs:Class rdf:about="http://www.e-editiones.ch/ontology/human-geography#District">
    <rdfs:isDefinedBy rdf:resource="http://www.e-editiones.ch/ontology/human-geography#"/>
    <rdfs:label xml:lang="en">district</rdfs:label>
    <rdfs:label xml:lang="de">Stadtviertel</rdfs:label>
    <rdfs:comment xml:lang="en">Part of a city with own administration.</rdfs:comment>
    <rdfs:subClassOf rdf:resource="http://www.e-editiones.ch/ontology/physical-entity#EarthPlace"/>
  </rdfs:Class>

  <rdfs:Class rdf:about="http://www.e-editiones.ch/ontology/human-geography#Street">
    <rdfs:isDefinedBy rdf:resource="http://www.e-editiones.ch/ontology/human-geography#"/>
    <rdfs:label xml:lang="en">street</rdfs:label>
    <rdfs:label xml:lang="de">Strasse</rdfs:label>
    <rdfs:comment xml:lang="en">.</rdfs:comment>
    <rdfs:subClassOf rdf:resource="http://www.e-editiones.ch/ontology/physical-entity#EarthPlace"/>
  </rdfs:Class>

  <rdfs:Class rdf:about="http://www.e-editiones.ch/ontology/human-geography#StreetPlace">
    <rdfs:isDefinedBy rdf:resource="http://www.e-editiones.ch/ontology/human-geography#"/>
    <rdfs:label xml:lang="en">street place</rdfs:label>
    <rdfs:label xml:lang="de">Strassenplatz</rdfs:label>
    <rdfs:comment xml:lang="en">.</rdfs:comment>
    <rdfs:subClassOf rdf:resource="http://www.e-editiones.ch/ontology/physical-entity#EarthPlace"/>
  </rdfs:Class>

  <rdfs:Class rdf:about="http://www.e-editiones.ch/ontology/human-geography#PostalAddress">
    <rdfs:isDefinedBy rdf:resource="http://www.e-editiones.ch/ontology/human-geography#"/>
    <rdfs:label xml:lang="en">postal address</rdfs:label>
    <rdfs:label xml:lang="de">Postanschrift</rdfs:label>
    <rdfs:comment xml:lang="en">.</rdfs:comment>
    <rdfs:subClassOf rdf:resource="http://www.e-editiones.ch/ontology/concept#SymbolicEntity"/>
  </rdfs:Class>

  <owl:ObjectProperty rdf:about="http://www.e-editiones.ch/ontology/human-geography#hasDomicile">
    <rdfs:isDefinedBy rdf:resource="http://www.e-editiones.ch/ontology/human-geography#"/>
    <rdfs:label xml:lang="en">has domicile</rdfs:label>
    <rdfs:label xml:lang="de">hat Sitz</rdfs:label>
    <rdfs:comment xml:lang="en">Relating a human agent - person or group - to a place of registered staying and contactability.</rdfs:comment>
    <rdfs:domain rdf:resource="http://www.e-editiones.ch/ontology/human#Agent"/>
    <rdfs:range rdf:resource="http://www.e-editiones.ch/ontology/physical-entity#Place"/>
    <rdfs:subPropertyOf rdf:resource="http://www.e-editiones.ch/ontology/physical-entity#hasLocation"/>
  </owl:ObjectProperty>

  <owl:ObjectProperty rdf:about="http://www.e-editiones.ch/ontology/human-geography#hasPostalAddress">
    <rdfs:isDefinedBy rdf:resource="http://www.e-editiones.ch/ontology/human-geography#"/>
    <rdfs:label xml:lang="en">has postal address</rdfs:label>
    <rdfs:label xml:lang="de">hat Postanschrift</rdfs:label>
    <rdfs:comment xml:lang="en">Relating someone or something to her/his/its postal address.</rdfs:comment>
    <rdfs:domain rdf:resource="http://www.w3.org/2000/01/rdf-schema#Resource"/>
    <rdfs:range rdf:resource="http://www.e-editiones.ch/ontology/human-geography#PostalAddress"/>
  </owl:ObjectProperty>

  <owl:ObjectProperty rdf:about="http://www.e-editiones.ch/ontology/human-geography#hasReceiverPostalAddress">
    <rdfs:isDefinedBy rdf:resource="http://www.e-editiones.ch/ontology/human-geography#"/>
    <rdfs:label xml:lang="en">has receiver postal address</rdfs:label>
    <rdfs:label xml:lang="de">hat Empf채nger-Postanschrift</rdfs:label>
    <rdfs:comment xml:lang="en">Relating something being sent to the postal address of the receiver.</rdfs:comment>
    <rdfs:domain rdf:resource="http://www.e-editiones.ch/ontology/physical-entity#Matter"/>
    <rdfs:range rdf:resource="http://www.e-editiones.ch/ontology/human-geography#PostalAddress"/>
  </owl:ObjectProperty>

  <owl:ObjectProperty rdf:about="http://www.e-editiones.ch/ontology/human-geography#hasReturnPostalAddress">
    <rdfs:isDefinedBy rdf:resource="http://www.e-editiones.ch/ontology/human-geography#"/>
    <rdfs:label xml:lang="en">has return postal address</rdfs:label>
    <rdfs:label xml:lang="de">hat R체cksendung-Postanschrift</rdfs:label>
    <rdfs:comment xml:lang="en">Relating something being sent to the postal address for return.</rdfs:comment>
    <rdfs:domain rdf:resource="http://www.e-editiones.ch/ontology/physical-entity#Matter"/>
    <rdfs:range rdf:resource="http://www.e-editiones.ch/ontology/human-geography#PostalAddress"/>
  </owl:ObjectProperty>

  <owl:DatatypeProperty rdf:about="http://www.e-editiones.ch/ontology/human-geography#addressHasAddresseeNameLiteral">
    <rdfs:isDefinedBy rdf:resource="http://www.e-editiones.ch/ontology/human-geography#"/>
    <rdfs:label xml:lang="en">postal address has addressee name literal</rdfs:label>
    <rdfs:label xml:lang="de">Postanschrift hat AdressatIns Nameliterale</rdfs:label>
    <rdfs:comment xml:lang="en">Relating a postal address to an addressee name literal.</rdfs:comment>
    <rdfs:domain rdf:resource="http://www.e-editiones.ch/ontology/human-geography#PostalAddress"/>
    <rdfs:range rdf:resource="http://www.w3.org/2000/01/rdf-schema#Literal"/>
    <rdfs:subPropertyOf rdf:resource="http://www.e-editiones.ch/ontology/text#hasNameLiteral"/>
  </owl:DatatypeProperty>

  <owl:DatatypeProperty rdf:about="http://www.e-editiones.ch/ontology/human-geography#addressHasStreetNameLiteral">
    <rdfs:isDefinedBy rdf:resource="http://www.e-editiones.ch/ontology/human-geography#"/>
    <rdfs:label xml:lang="en">postal address has street name literal</rdfs:label>
    <rdfs:label xml:lang="de">Postanschrift hat Strassennameliterale</rdfs:label>
    <rdfs:comment xml:lang="en">Relating a postal address to the name literal of the street.</rdfs:comment>
    <rdfs:domain rdf:resource="http://www.e-editiones.ch/ontology/human-geography#PostalAddress"/>
    <rdfs:range rdf:resource="http://www.w3.org/2000/01/rdf-schema#Literal"/>
    <rdfs:subPropertyOf rdf:resource="http://www.e-editiones.ch/ontology/text#hasNameLiteral"/>
  </owl:DatatypeProperty>

  <owl:DatatypeProperty rdf:about="http://www.e-editiones.ch/ontology/human-geography#addressHasHouseNumberNumeral">
    <rdfs:isDefinedBy rdf:resource="http://www.e-editiones.ch/ontology/human-geography#"/>
    <rdfs:label xml:lang="en">postal address has house number numeral</rdfs:label>
    <rdfs:label xml:lang="de">Postanschrift hat Hausnummernumerale</rdfs:label>
    <rdfs:comment xml:lang="en">Relating a postal address to the nummer numeral of the house.</rdfs:comment>
    <rdfs:domain rdf:resource="http://www.e-editiones.ch/ontology/human-geography#PostalAddress"/>
    <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#positiveInteger"/>
    <rdfs:subPropertyOf rdf:resource="http://www.e-editiones.ch/ontology/mathematics#hasOrdinalNumeral"/>
  </owl:DatatypeProperty>

  <owl:DatatypeProperty rdf:about="http://www.e-editiones.ch/ontology/human-geography#addressHasPostcodeValue">
    <rdfs:isDefinedBy rdf:resource="http://www.e-editiones.ch/ontology/human-geography#"/>
    <rdfs:label xml:lang="en">postal address has postcode value</rdfs:label>
    <rdfs:label xml:lang="de">Postanschrift hat Postleitzahlwert</rdfs:label>
    <rdfs:comment xml:lang="en">Relating a postal address to its postcode value.</rdfs:comment>
    <rdfs:domain rdf:resource="http://www.e-editiones.ch/ontology/human-geography#PostalAddress"/>
    <rdfs:range rdf:resource="http://www.w3.org/2000/01/rdf-schema#Literal"/>
    <rdfs:subPropertyOf rdf:resource="http://www.e-editiones.ch/ontology/concept#hasLiteral"/>
  </owl:DatatypeProperty>

  <owl:DatatypeProperty rdf:about="http://www.e-editiones.ch/ontology/human-geography#addressHasTownNameLiteral">
    <rdfs:isDefinedBy rdf:resource="http://www.e-editiones.ch/ontology/human-geography#"/>
    <rdfs:label xml:lang="en">postal address has town name literal</rdfs:label>
    <rdfs:label xml:lang="de">Postanschrift hat Ortsnameliterale</rdfs:label>
    <rdfs:comment xml:lang="en">Relating a postal address to its town name literal.</rdfs:comment>
    <rdfs:domain rdf:resource="http://www.e-editiones.ch/ontology/human-geography#PostalAddress"/>
    <rdfs:range rdf:resource="http://www.w3.org/2000/01/rdf-schema#Literal"/>
    <rdfs:subPropertyOf rdf:resource="http://www.e-editiones.ch/ontology/text#hasNameLiteral"/>
  </owl:DatatypeProperty>

  <owl:DatatypeProperty rdf:about="http://www.e-editiones.ch/ontology/human-geography#addressHasCountryNameLiteral">
    <rdfs:isDefinedBy rdf:resource="http://www.e-editiones.ch/ontology/human-geography#"/>
    <rdfs:label xml:lang="en">postal address has country name literal</rdfs:label>
    <rdfs:label xml:lang="de">Postanschrift hat L채ndernameliterale</rdfs:label>
    <rdfs:comment xml:lang="en">Relating a postal address to its country name literal.</rdfs:comment>
    <rdfs:domain rdf:resource="http://www.e-editiones.ch/ontology/human-geography#PostalAddress"/>
    <rdfs:range rdf:resource="http://www.w3.org/2000/01/rdf-schema#Literal"/>
    <rdfs:subPropertyOf rdf:resource="http://www.e-editiones.ch/ontology/text#hasNameLiteral"/>
  </owl:DatatypeProperty>

</rdf:RDF>

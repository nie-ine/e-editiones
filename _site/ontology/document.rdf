>
  <owl:DatatypeProperty rdf:about="http://e-editiones.ch/ontology/document#expressionHasSubtitleLiteral">
    <rdfs:range rdf:resource="http://www.w3.org/2000/01/rdf-schema#Literal"/>
    <rdfs:isDefinedBy rdf:resource="http://e-editiones.ch/ontology/document#"/>
    <rdfs:subPropertyOf rdf:resource="http://e-editiones.ch/ontology/text#hasSubtitleLiteral"/>
    <rdfs:label xml:lang="de">Expression hat Untertitelliteral</rdfs:label>
    <rdfs:domain rdf:resource="http://e-editiones.ch/ontology/document#Expression"/>
    <rdfs:comment xml:lang="en">Relating an expression to a subtitle literal it has.</rdfs:comment>
    <rdfs:label xml:lang="en">expression has subtitle literal</rdfs:label>
  </owl:DatatypeProperty>
  <owl:ObjectProperty rdf:about="http://e-editiones.ch/ontology/document#isColumnOfPage">
    <rdfs:comment xml:lang="en">Relating a column to a document page it is part of.</rdfs:comment>
    <rdfs:label xml:lang="de">ist Spalte einer Dokumentseite</rdfs:label>
    <rdfs:label xml:lang="en">is column of document page</rdfs:label>
    <rdfs:isDefinedBy rdf:resource="http://e-editiones.ch/ontology/document#"/>
    <rdfs:domain rdf:resource="http://e-editiones.ch/ontology/document#Column"/>
    <rdfs:subPropertyOf rdf:resource="http://e-editiones.ch/ontology/concept#isPartOf"/>
    <rdfs:range rdf:resource="http://e-editiones.ch/ontology/document#Page"/>
  </owl:ObjectProperty>
  <owl:ObjectProperty rdf:about="http://e-editiones.ch/ontology/document#isPageOfExpression">
    <rdfs:subPropertyOf rdf:resource="http://e-editiones.ch/ontology/concept#isPartOf"/>
    <rdfs:domain rdf:resource="http://e-editiones.ch/ontology/document#Page"/>
    <rdfs:range rdf:resource="http://e-editiones.ch/ontology/document#Expression"/>
    <rdfs:isDefinedBy rdf:resource="http://e-editiones.ch/ontology/document#"/>
    <rdfs:label xml:lang="en">is page of document expression</rdfs:label>
    <rdfs:comment xml:lang="en">Relating a document page to a document expression it is part of.</rdfs:comment>
    <rdfs:label xml:lang="de">ist Seite einer Dokumentexpression</rdfs:label>
  </owl:ObjectProperty>
  <owl:ObjectProperty rdf:about="http://e-editiones.ch/ontology/document#contentStructureHasFootnoteReference">
    <rdfs:isDefinedBy rdf:resource="http://e-editiones.ch/ontology/document#"/>
    <rdfs:comment xml:lang="en">Relating a document content structure to a footnote reference it has.</rdfs:comment>
    <rdfs:label xml:lang="en">document content structure has footnote reference</rdfs:label>
    <rdfs:label xml:lang="de">Dokumentinhaltstruktur hat Fussnotenreferenz</rdfs:label>
    <rdfs:domain rdf:resource="http://e-editiones.ch/ontology/document#ContentStructure"/>
    <rdfs:range rdf:resource="http://e-editiones.ch/ontology/document#FootnoteReference"/>
    <rdfs:subPropertyOf>
      <owl:ObjectProperty rdf:about="http://e-editiones.ch/ontology/document#contentStructureHasTextReference">
        <rdfs:label xml:lang="de">Dokumentinhaltstruktur hat Textreferenz</rdfs:label>
        <rdfs:label xml:lang="en">document content structure has text reference</rdfs:label>
        <rdfs:subPropertyOf rdf:resource="http://e-editiones.ch/ontology/document#contentStructureHasReference"/>
        <rdfs:domain rdf:resource="http://e-editiones.ch/ontology/document#ContentStructure"/>
        <rdfs:isDefinedBy rdf:resource="http://e-editiones.ch/ontology/document#"/>
        <rdfs:range rdf:resource="http://e-editiones.ch/ontology/document#TextStructureReference"/>
        <rdfs:comment xml:lang="en">Relating a document content structure to a text reference it has.</rdfs:comment>
      </owl:ObjectProperty>
    </rdfs:subPropertyOf>
  </owl:ObjectProperty>
  <rdfs:Class rdf:about="http://e-editiones.ch/ontology/document#Draft">
    <rdfs:subClassOf rdf:resource="http://e-editiones.ch/ontology/document#Expression"/>
    <rdfs:isDefinedBy rdf:resource="http://e-editiones.ch/ontology/document#"/>
    <rdfs:label xml:lang="en">document draft</rdfs:label>
    <rdfs:subClassOf rdf:resource="http://e-editiones.ch/ontology/creating#UnfinishedCreation"/>
    <rdfs:label xml:lang="de">Dokumententwurf</rdfs:label>
    <rdfs:comment xml:lang="en">Developmental document expression, subject to future modification, not having all envisioned features.</rdfs:comment>
  </rdfs:Class>
  <owl:Ontology rdf:about="http://e-editiones.ch/ontology/document">
    <dct:description xml:lang="en">Formal description of document, general subclasses, related classes and properties.</dct:description>
    <dct:title xml:lang="en">An ontology about document</dct:title>
    <dct:creator xml:lang="en">Hans Cools, MD, knowledge engineer, ontologist, software developer, University of Basel, Switzerland</dct:creator>
    <owl:versionInfo rdf:datatype="http://www.w3.org/2001/XMLSchema#date">2020-06-11</owl:versionInfo>
    <dct:contributor xml:lang="en">Rebekka Plüss, research assistant and software developer, University of Basel, Switzerland</dct:contributor>
    <dct:contributor xml:lang="en">Roberta Padlina, project coordinator and software developer, University of Basel, Switzerland</dct:contributor>
    <dct:publisher xml:lang="en">Universities of Basel, Bern, and Zürich, Switzerland</dct:publisher>
    <dct:license rdf:resource="http://creativecommons.org/licenses/by/3.0/"/>
  </owl:Ontology>
  <owl:ObjectProperty rdf:about="http://e-editiones.ch/ontology/document#isTableOfColumn">
    <rdfs:label xml:lang="en">is table of column</rdfs:label>
    <rdfs:label xml:lang="de">ist Tabelle einer Spalte</rdfs:label>
    <rdfs:domain rdf:resource="http://e-editiones.ch/ontology/document#Table"/>
    <rdfs:comment xml:lang="en">Relating a table to a column it is part of.</rdfs:comment>
    <rdfs:subPropertyOf rdf:resource="http://e-editiones.ch/ontology/concept#isPartOf"/>
    <rdfs:isDefinedBy rdf:resource="http://e-editiones.ch/ontology/document#"/>
    <rdfs:range rdf:resource="http://e-editiones.ch/ontology/document#Column"/>
  </owl:ObjectProperty>
  <owl:ObjectProperty rdf:about="http://e-editiones.ch/ontology/document#isSpaceOfDocument">
    <rdfs:label xml:lang="de">ist Leerraum in einem Dokument</rdfs:label>
    <rdfs:label xml:lang="en">is space of document</rdfs:label>
    <rdfs:isDefinedBy rdf:resource="http://e-editiones.ch/ontology/document#"/>
    <rdfs:domain rdf:resource="http://e-editiones.ch/ontology/document#Space"/>
    <rdfs:range rdf:resource="http://e-editiones.ch/ontology/document#Document"/>
    <rdfs:subPropertyOf>
      <owl:ObjectProperty rdf:about="http://e-editiones.ch/ontology/document#isStructureOfDocument">
        <rdfs:label xml:lang="en">is structure of document</rdfs:label>
        <rdfs:comment xml:lang="en">Relating a document structure to a document it is part of.</rdfs:comment>
        <rdfs:range rdf:resource="http://e-editiones.ch/ontology/document#Document"/>
        <rdfs:subPropertyOf rdf:resource="http://e-editiones.ch/ontology/document#isElementOfDocument"/>
        <rdfs:label xml:lang="de">ist Struktur eines Dokuments</rdfs:label>
        <rdfs:domain rdf:resource="http://e-editiones.ch/ontology/document#Structure"/>
        <rdfs:isDefinedBy rdf:resource="http://e-editiones.ch/ontology/document#"/>
      </owl:ObjectProperty>
    </rdfs:subPropertyOf>
    <rdfs:comment xml:lang="en">Relating a space to the document it is part of.</rdfs:comment>
  </owl:ObjectProperty>
  <owl:ObjectProperty rdf:about="http://e-editiones.ch/ontology/document#contentStructureHasExpressionReference">
    <rdfs:domain rdf:resource="http://e-editiones.ch/ontology/document#ContentStructure"/>
    <rdfs:range rdf:resource="http://e-editiones.ch/ontology/document#ExpressionReference"/>
    <rdfs:comment xml:lang="en">Relating a document content structure to an expression reference it has.</rdfs:comment>
    <rdfs:isDefinedBy rdf:resource="http://e-editiones.ch/ontology/document#"/>
    <rdfs:label xml:lang="de">Dokumentinhaltstruktur hat Expressionsreferenz</rdfs:label>
    <rdfs:label xml:lang="en">document content structure has expression reference</rdfs:label>
  </owl:ObjectProperty>
  <rdfs:Class rdf:about="http://e-editiones.ch/ontology/document#PolyAuthorExpression">
    <rdfs:isDefinedBy rdf:resource="http://e-editiones.ch/ontology/document#"/>
    <rdfs:subClassOf rdf:resource="http://e-editiones.ch/ontology/document#Expression"/>
    <rdfs:comment xml:lang="en">Document expression by different authors.</rdfs:comment>
    <rdfs:label xml:lang="en">poly-author document expression</rdfs:label>
    <rdfs:label xml:lang="de">Mehrautorendokumentexpression</rdfs:label>
  </rdfs:Class>
  <owl:ObjectProperty rdf:about="http://e-editiones.ch/ontology/document#contentStructureHasEndnote">
    <owl:propertyChainAxiom rdf:parseType="Collection">
      <rdf:Description rdf:about="http://e-editiones.ch/ontology/document#contentStructureHasEndnoteReference"/>
      <rdf:Description rdf:about="http://e-editiones.ch/ontology/document#endnoteReferenceHasIdentifier"/>
      <rdf:Description rdf:about="http://e-editiones.ch/ontology/note-structure#isIdentifierOfEndnote"/>
    </owl:propertyChainAxiom>
    <rdfs:comment xml:lang="en">Relating a document content structure to an endnote it has.</rdfs:comment>
    <rdfs:domain rdf:resource="http://e-editiones.ch/ontology/document#ContentStructure"/>
    <rdfs:label xml:lang="en">document content structure has endnote</rdfs:label>
    <rdfs:label xml:lang="de">Dokumentinhaltstruktur hat Endnote</rdfs:label>
    <rdfs:range rdf:resource="http://e-editiones.ch/ontology/document#Endnote"/>
    <rdfs:isDefinedBy rdf:resource="http://e-editiones.ch/ontology/document#"/>
  </owl:ObjectProperty>
  <owl:ObjectProperty rdf:about="http://e-editiones.ch/ontology/document#footnoteReferenceHasFootnoteNumber">
    <rdfs:label xml:lang="de">Fussnotenreferenz hat Fussnotennummer</rdfs:label>
    <rdfs:subPropertyOf>
      <owl:ObjectProperty rdf:about="http://e-editiones.ch/ontology/document#footnoteReferenceHasFootnoteIdentifier">
        <rdfs:isDefinedBy rdf:resource="http://e-editiones.ch/ontology/document#"/>
        <rdfs:label xml:lang="en">footnote reference has footnote identifier</rdfs:label>
        <rdfs:comment xml:lang="en">Relating a footnote reference of a document content structure to the identifier of the footnote.</rdfs:comment>
        <rdfs:domain rdf:resource="http://e-editiones.ch/ontology/document#FootnoteReference"/>
        <rdfs:subPropertyOf rdf:resource="http://e-editiones.ch/ontology/document#textStructureReferenceHasTextStructureIdentifier"/>
        <rdfs:range rdf:resource="http://e-editiones.ch/ontology/note-structure#FootnoteIdentifier"/>
        <rdfs:label xml:lang="de">Fussnotenreferenz hat Fussnotenidentifikator</rdfs:label>
      </owl:ObjectProperty>
    </rdfs:subPropertyOf>
    <rdfs:domain rdf:resource="http://e-editiones.ch/ontology/document#FootnoteReference"/>
    <rdfs:comment xml:lang="en">Relating a footnote reference of a document content structure to the number of the footnote.</rdfs:comment>
    <rdfs:isDefinedBy rdf:resource="http://e-editiones.ch/ontology/document#"/>
    <rdfs:range rdf:resource="http://e-editiones.ch/ontology/note-structure#FootnoteNumber"/>
    <rdfs:label xml:lang="en">footnote reference has footnote number</rdfs:label>
  </owl:ObjectProperty>
  <owl:ObjectProperty rdf:about="http://e-editiones.ch/ontology/document#hasContentLiteral">
    <rdfs:domain rdf:resource="http://e-editiones.ch/ontology/document#Document"/>
    <rdfs:label xml:lang="en">document has content literal</rdfs:label>
    <rdfs:isDefinedBy rdf:resource="http://e-editiones.ch/ontology/document#"/>
    <rdfs:range rdf:resource="http://www.w3.org/2000/01/rdf-schema#Literal"/>
    <skos:note xml:lang="en">Includes a code literal of a programming language, e.g. HTML.</skos:note>
    <rdfs:comment xml:lang="en">Relating a document to its content literal.</rdfs:comment>
    <rdfs:label xml:lang="de">Dokument hat Inhaltliteral</rdfs:label>
  </owl:ObjectProperty>
  <rdfs:Class rdf:about="http://e-editiones.ch/ontology/document#CompleteWorks">
    <rdfs:label xml:lang="de">Gesamtwerk</rdfs:label>
    <rdfs:subClassOf rdf:resource="http://e-editiones.ch/ontology/document#Set"/>
    <rdfs:comment xml:lang="en">Set of individual document expressions of one author.</rdfs:comment>
    <rdfs:isDefinedBy rdf:resource="http://e-editiones.ch/ontology/document#"/>
    <rdfs:label xml:lang="en">complete works</rdfs:label>
  </rdfs:Class>
  <owl:DatatypeProperty rdf:about="http://e-editiones.ch/ontology/document#pageHasOrdinalNumeral">
    <rdfs:label xml:lang="de">Seite hat Ordinalnumeral</rdfs:label>
    <rdfs:label xml:lang="en">page has ordinal numeral</rdfs:label>
    <rdfs:isDefinedBy rdf:resource="http://e-editiones.ch/ontology/document#"/>
    <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#nonNegativeInteger"/>
    <rdfs:comment xml:lang="en">Relating a page to its ordinal numeral.</rdfs:comment>
    <rdfs:subPropertyOf rdf:resource="http://e-editiones.ch/ontology/mathematics#hasOrdinalNumeral"/>
    <rdfs:domain rdf:resource="http://e-editiones.ch/ontology/document#Page"/>
    <owl:propertyChainAxiom rdf:parseType="Collection">
      <rdf:Description rdf:about="http://e-editiones.ch/ontology/document#pageHasOrdinalNumber"/>
      <rdf:Description rdf:about="http://e-editiones.ch/ontology/mathematics#hasNumeral"/>
    </owl:propertyChainAxiom>
  </owl:DatatypeProperty>
  <rdfs:Class rdf:about="http://e-editiones.ch/ontology/document#Layout">
    <rdfs:subClassOf rdf:resource="http://e-editiones.ch/ontology/document#Element"/>
    <rdfs:isDefinedBy rdf:resource="http://e-editiones.ch/ontology/document#"/>
    <rdfs:label xml:lang="de">Layout</rdfs:label>
    <rdfs:comment xml:lang="en">Way in which the content of a page is set out.</rdfs:comment>
    <rdfs:label xml:lang="en">layout</rdfs:label>
  </rdfs:Class>
  <owl:ObjectProperty rdf:about="http://e-editiones.ch/ontology/document#isExpressionOfCycle">
    <rdfs:subPropertyOf rdf:resource="http://e-editiones.ch/ontology/concept#isPartOf"/>
    <rdfs:isDefinedBy rdf:resource="http://e-editiones.ch/ontology/document#"/>
    <rdfs:label xml:lang="de">ist Expression eines Zyklus</rdfs:label>
    <rdfs:range rdf:resource="http://e-editiones.ch/ontology/document#Cycle"/>
    <rdfs:domain rdf:resource="http://e-editiones.ch/ontology/document#IndividualExpression"/>
    <rdfs:label xml:lang="en">is expression of cycle</rdfs:label>
    <rdfs:comment xml:lang="en">Relating an individual expression to a cycle it is part of.</rdfs:comment>
  </owl:ObjectProperty>
  <owl:ObjectProperty rdf:about="http://e-editiones.ch/ontology/document#endnoteReferenceHasEndnoteNumber">
    <rdfs:comment xml:lang="en">Relating an endnote reference of a document content structure to the number of the endnote.</rdfs:comment>
    <rdfs:range rdf:resource="http://e-editiones.ch/ontology/note-structure#EndnoteNumber"/>
    <rdfs:isDefinedBy rdf:resource="http://e-editiones.ch/ontology/document#"/>
    <rdfs:label xml:lang="en">endnote reference has endnote number</rdfs:label>
    <rdfs:subPropertyOf>
      <owl:ObjectProperty rdf:about="http://e-editiones.ch/ontology/document#endnoteReferenceHasEndnoteIdentifier">
        <rdfs:comment xml:lang="en">Relating an endnote reference of a document content structure to the identifier of the endnote.</rdfs:comment>
        <rdfs:domain rdf:resource="http://e-editiones.ch/ontology/document#EndnoteReference"/>
        <rdfs:isDefinedBy rdf:resource="http://e-editiones.ch/ontology/document#"/>
        <rdfs:label xml:lang="de">Endnotenreferenz hat Endnotenidentifikator</rdfs:label>
        <rdfs:range rdf:resource="http://e-editiones.ch/ontology/note-structure#EndnoteIdentifier"/>
        <rdfs:subPropertyOf rdf:resource="http://e-editiones.ch/ontology/document#textStructureReferenceHasTextStructureIdentifier"/>
        <rdfs:label xml:lang="en">endnote reference has endnote identifier</rdfs:label>
      </owl:ObjectProperty>
    </rdfs:subPropertyOf>
    <rdfs:label xml:lang="de">Endnotenreferenz hat Endnotennummer</rdfs:label>
    <rdfs:domain rdf:resource="http://e-editiones.ch/ontology/document#EndnoteReference"/>
  </owl:ObjectProperty>
  <owl:ObjectProperty rdf:about="http://e-editiones.ch/ontology/document#expressionReferenceHasIdentifier">
    <rdfs:isDefinedBy rdf:resource="http://e-editiones.ch/ontology/document#"/>
    <rdfs:label xml:lang="de">Expressionsreferenz hat Identifikator</rdfs:label>
    <rdfs:label xml:lang="en">expression reference has identifier</rdfs:label>
    <rdfs:domain rdf:resource="http://e-editiones.ch/ontology/document#ExpressionReference"/>
    <rdfs:comment xml:lang="en">Relating a document expression reference to its identifier.</rdfs:comment>
    <rdfs:range rdf:resource="http://e-editiones.ch/ontology/document#ExpressionIdentifier"/>
    <rdfs:subPropertyOf rdf:resource="http://e-editiones.ch/ontology/concept#hasIdentifier"/>
  </owl:ObjectProperty>
  <owl:ObjectProperty rdf:about="http://e-editiones.ch/ontology/document#isPartOfIndividualExpression">
    <rdfs:range rdf:resource="http://e-editiones.ch/ontology/document#IndividualExpression"/>
    <rdfs:domain rdf:resource="http://e-editiones.ch/ontology/document#ExpressionPart"/>
    <rdfs:label xml:lang="de">ist Teil einer individuellen Expression</rdfs:label>
    <rdfs:subPropertyOf rdf:resource="http://e-editiones.ch/ontology/concept#isPartOf"/>
    <rdfs:comment xml:lang="en">Relating an expression part to an individual expression it is part of.</rdfs:comment>
    <rdfs:label xml:lang="en">is part of individual expression</rdfs:label>
    <rdfs:isDefinedBy rdf:resource="http://e-editiones.ch/ontology/document#"/>
  </owl:ObjectProperty>
  <owl:ObjectProperty rdf:about="http://e-editiones.ch/ontology/document#hasCitationBibliographicReference">
    <rdfs:domain rdf:resource="http://e-editiones.ch/ontology/text-expression#Expression"/>
    <rdfs:label xml:lang="en">has citation bibliographic reference</rdfs:label>
    <rdfs:subPropertyOf rdf:resource="http://e-editiones.ch/ontology/concept#hasSymbolicEntity"/>
    <rdfs:isDefinedBy rdf:resource="http://e-editiones.ch/ontology/document#"/>
    <rdfs:range rdf:resource="http://e-editiones.ch/ontology/document#CitationBibliographicReference"/>
    <rdfs:label xml:lang="de">hat bibliographische Referenz eines Zitats</rdfs:label>
    <rdfs:comment xml:lang="en">Relating a text expression to a bibliographic reference of a citation thereof.</rdfs:comment>
  </owl:ObjectProperty>
  <owl:ObjectProperty rdf:about="http://e-editiones.ch/ontology/document#hasState">
    <rdfs:isDefinedBy rdf:resource="http://e-editiones.ch/ontology/document#"/>
    <rdfs:label xml:lang="de">Dokument hat Zustand</rdfs:label>
    <rdfs:range rdf:resource="http://e-editiones.ch/ontology/document#State"/>
    <rdfs:comment xml:lang="en">Relating a document to its state.</rdfs:comment>
    <rdfs:label xml:lang="en">document has state</rdfs:label>
    <rdfs:domain rdf:resource="http://e-editiones.ch/ontology/document#Document"/>
  </owl:ObjectProperty>
  <owl:DatatypeProperty rdf:about="http://e-editiones.ch/ontology/document#hasBibliographicReferenceLiteral">
    <rdfs:comment xml:lang="en">Relating a document expression to the literal of a bibliographic reference it has.</rdfs:comment>
    <rdfs:label xml:lang="de">hat bibliographisches Referenzliteral</rdfs:label>
    <rdfs:label xml:lang="it">ha riferimenti bibliografici letterali</rdfs:label>
    <owl:propertyChainAxiom rdf:parseType="Collection">
      <rdf:Description rdf:about="http://e-editiones.ch/ontology/document#hasBibliographicReference"/>
      <rdf:Description rdf:about="http://e-editiones.ch/ontology/document#bibliographicReferenceHasLiteral"/>
    </owl:propertyChainAxiom>
    <rdfs:isDefinedBy rdf:resource="http://e-editiones.ch/ontology/document#"/>
    <rdfs:label xml:lang="en">has bibliographic reference literal</rdfs:label>
    <rdfs:label xml:lang="fr">a un littéral de référence bibliographique</rdfs:label>
    <rdfs:subPropertyOf rdf:resource="http://e-editiones.ch/ontology/concept#hasLiteral"/>
    <rdfs:domain rdf:resource="http://e-editiones.ch/ontology/document#Expression"/>
    <rdfs:range rdf:resource="http://www.w3.org/2000/01/rdf-schema#Literal"/>
  </owl:DatatypeProperty>
  <rdfs:Class rdf:about="http://e-editiones.ch/ontology/document#VerticalSpace">

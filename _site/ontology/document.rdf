    <rdfs:label xml:lang="en">is hypertext document on webpage</rdfs:label>
    <rdfs:label xml:lang="de">ist Hypertext-Dokument auf Webseite</rdfs:label>
    <rdfs:isDefinedBy rdf:resource="http://e-editiones.ch/ontology/document#"/>
    <rdfs:domain rdf:resource="http://e-editiones.ch/ontology/document#HypertextDocument"/>

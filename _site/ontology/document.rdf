    <rdfs:subPropertyOf rdf:resource="http://e-editiones.ch/ontology/concept#hasLiteral"/>
    <rdfs:label xml:lang="de">hat CSS-Literale</rdfs:label>
    <rdfs:domain rdf:resource="http://www.w3.org/2000/01/rdf-schema#Resource"/>
  </owl:DatatypeProperty>
  <rdfs:Class rdf:about="http://e-editiones.ch/ontology/document#TitlePage">
    <rdfs:label xml:lang="en">title page</rdfs:label>

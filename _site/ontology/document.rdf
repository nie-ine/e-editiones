        <rdfs:subPropertyOf rdf:resource="http://e-editiones.ch/ontology/document#hasTextStructureIdentifier"/>
        <rdfs:label xml:lang="de">hat Endnoteidentifikator</rdfs:label>
        <rdfs:label xml:lang="en">has endnote identifier</rdfs:label>
        <rdfs:comment xml:lang="en">Relating an endnote reference to its endnote identifier.</rdfs:comment>
        <rdfs:range rdf:resource="http://e-editiones.ch/ontology/note-structure#EndnoteIdentifier"/>
      </owl:ObjectProperty>
    </rdfs:subPropertyOf>
    <rdfs:label xml:lang="de">hat Endnotenummer</rdfs:label>
    <rdfs:domain rdf:resource="http://e-editiones.ch/ontology/document#EndnoteReference"/>
    <rdfs:isDefinedBy rdf:resource="http://e-editiones.ch/ontology/document#"/>
    <rdfs:label xml:lang="en">has endnote number</rdfs:label>
  </owl:ObjectProperty>

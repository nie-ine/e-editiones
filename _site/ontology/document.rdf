    <rdfs:subPropertyOf rdf:resource="http://e-editiones.ch/ontology/concept#hasLiteral"/>
  </owl:DatatypeProperty>
  <owl:DatatypeProperty rdf:about="http://e-editiones.ch/ontology/document#expressionHasTitleLiteral">

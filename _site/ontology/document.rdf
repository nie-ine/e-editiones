    <rdfs:domain rdf:resource="http://e-editiones.ch/ontology/document#ContentStructure"/>
    <rdfs:label xml:lang="en">has expression reference</rdfs:label>
  </owl:ObjectProperty>
  <owl:ObjectProperty rdf:about="http://e-editiones.ch/ontology/document#hasFootnoteNumber">
    <rdfs:isDefinedBy rdf:resource="http://e-editiones.ch/ontology/document#"/>
    <rdfs:label xml:lang="de">hat Fussnotenummer</rdfs:label>

    <rdfs:label xml:lang="de">Expression hat Titelliteral</rdfs:label>
    <rdfs:comment xml:lang="en">Relating an expression to its title literal.</rdfs:comment>
    <rdfs:isDefinedBy rdf:resource="http://e-editiones.ch/ontology/document#"/>
    <rdfs:range rdf:resource="http://www.w3.org/2000/01/rdf-schema#Literal"/>

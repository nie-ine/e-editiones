    <rdfs:label xml:lang="en">has footnote reference</rdfs:label>
    <rdfs:subPropertyOf>
      <owl:ObjectProperty rdf:about="http://e-editiones.ch/ontology/document#hasTextReference">
        <rdfs:domain rdf:resource="http://e-editiones.ch/ontology/document#ContentStructure"/>
        <rdfs:range rdf:resource="http://e-editiones.ch/ontology/document#TextStructureReference"/>
        <rdfs:comment xml:lang="en">Relating a document content structure to a text reference it has.</rdfs:comment>
        <rdfs:label xml:lang="en">has text reference</rdfs:label>
        <rdfs:label xml:lang="de">hat Textreferenz</rdfs:label>
        <rdfs:subPropertyOf rdf:resource="http://e-editiones.ch/ontology/document#hasReference"/>
        <rdfs:isDefinedBy rdf:resource="http://e-editiones.ch/ontology/document#"/>
      </owl:ObjectProperty>
    </rdfs:subPropertyOf>
    <rdfs:domain rdf:resource="http://e-editiones.ch/ontology/document#ContentStructure"/>
    <rdfs:isDefinedBy rdf:resource="http://e-editiones.ch/ontology/document#"/>
    <rdfs:label xml:lang="de">hat Fussnotereferenz</rdfs:label>
  </owl:ObjectProperty>
  <rdfs:Class rdf:about="http://e-editiones.ch/ontology/document#Catalogue">
    <rdfs:label xml:lang="fr">catalogue</rdfs:label>
    <rdfs:label xml:lang="de">Katalog</rdfs:label>
    <rdfs:subClassOf rdf:resource="http://e-editiones.ch/ontology/document#IndividualExpression"/>
    <rdfs:isDefinedBy rdf:resource="http://e-editiones.ch/ontology/document#"/>
    <rdfs:comment xml:lang="en">List of descriptions of things, mostly ordered, e.g. alphabetically; e.g. library's book catalogue, exhibition's works of art catalogue, mail-order company's items for sale catalogue.</rdfs:comment>
    <rdfs:label xml:lang="en">catalogue</rdfs:label>
  </rdfs:Class>
  <owl:ObjectProperty rdf:about="http://e-editiones.ch/ontology/document#expressionReferenceHasIdentifier">
    <rdfs:isDefinedBy rdf:resource="http://e-editiones.ch/ontology/document#"/>
    <rdfs:label xml:lang="de">Expression-Referenz hat Identifikator</rdfs:label>

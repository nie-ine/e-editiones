    <rdfs:label xml:lang="en">is table of column</rdfs:label>
    <rdfs:isDefinedBy rdf:resource="http://e-editiones.ch/ontology/document#"/>
  </owl:ObjectProperty>
  <owl:ObjectProperty rdf:about="http://e-editiones.ch/ontology/document#hasEndnote">
    <rdfs:label xml:lang="en">has endnote</rdfs:label>
    <rdfs:comment xml:lang="en">Relating a document content structure to an endnote it has.</rdfs:comment>
    <rdfs:isDefinedBy rdf:resource="http://e-editiones.ch/ontology/document#"/>
    <rdfs:domain rdf:resource="http://e-editiones.ch/ontology/document#ContentStructure"/>
    <owl:propertyChainAxiom rdf:parseType="Collection">
      <rdf:Description rdf:about="http://e-editiones.ch/ontology/document#hasEndnoteReference"/>
      <rdf:Description rdf:about="http://e-editiones.ch/ontology/document#hasEndnoteIdentifier"/>
      <rdf:Description rdf:about="http://e-editiones.ch/ontology/note-structure#isIdentifierOfEndnote"/>
    </owl:propertyChainAxiom>
    <rdfs:label xml:lang="de">hat Endnote</rdfs:label>
    <rdfs:range rdf:resource="http://e-editiones.ch/ontology/document#Endnote"/>
  </owl:ObjectProperty>
  <owl:DatatypeProperty rdf:about="http://e-editiones.ch/ontology/document#pageHasOrdinalNumeral">
    <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#nonNegativeInteger"/>
    <rdfs:isDefinedBy rdf:resource="http://e-editiones.ch/ontology/document#"/>

    <rdfs:isDefinedBy rdf:resource="http://e-editiones.ch/ontology/document#"/>
    <rdfs:range rdf:resource="http://e-editiones.ch/ontology/document#Document"/>
    <rdfs:label xml:lang="en">is space of document</rdfs:label>
  </owl:ObjectProperty>

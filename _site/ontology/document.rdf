    <rdfs:label xml:lang="en">complete works</rdfs:label>
    <rdfs:subClassOf rdf:resource="http://e-editiones.ch/ontology/document#Set"/>
  </rdfs:Class>
  <rdfs:Class rdf:about="http://e-editiones.ch/ontology/document#Encoder">
    <rdfs:label xml:lang="de">KodiererIn</rdfs:label>

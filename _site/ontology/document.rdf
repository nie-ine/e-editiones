    <rdfs:comment xml:lang="en">Relating a document expression reference to its identifier.</rdfs:comment>
    <rdfs:subPropertyOf rdf:resource="http://e-editiones.ch/ontology/concept#hasIdentifier"/>
    <rdfs:label xml:lang="en">expression reference has identifier</rdfs:label>
    <rdfs:domain rdf:resource="http://e-editiones.ch/ontology/document#ExpressionReference"/>
  </owl:ObjectProperty>
  <rdfs:Class rdf:about="http://e-editiones.ch/ontology/document#HorizontalSpace">

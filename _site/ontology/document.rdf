    <rdfs:comment xml:lang="en">Document page contianing a title.</rdfs:comment>
    <rdfs:subClassOf rdf:resource="http://e-editiones.ch/ontology/document#Page"/>
    <rdfs:label xml:lang="de">Titelblatt</rdfs:label>
  </rdfs:Class>
  <rdfs:Class rdf:about="http://e-editiones.ch/ontology/document#Marginal">
    <rdfs:isDefinedBy rdf:resource="http://e-editiones.ch/ontology/document#"/>
    <rdfs:label xml:lang="de">Marginale</rdfs:label>
    <rdfs:label xml:lang="en">marginal</rdfs:label>

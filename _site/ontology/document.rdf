    <rdfs:isDefinedBy rdf:resource="http://e-editiones.ch/ontology/document#"/>
    <rdfs:comment xml:lang="en">Document space with horizontal orientation, e.g. between lines.</rdfs:comment>
    <rdfs:label xml:lang="en">horizontal document space</rdfs:label>
    <rdfs:subClassOf rdf:resource="http://e-editiones.ch/ontology/document#Space"/>
  </rdfs:Class>
  <owl:ObjectProperty rdf:about="http://e-editiones.ch/ontology/document#hasEndnoteNumber">

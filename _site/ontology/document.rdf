    <rdfs:comment xml:lang="en">Information besides a text in the margin of a carrier, e.g. text, doodles, illuminations.</rdfs:comment>
  </rdfs:Class>
  <owl:ObjectProperty rdf:about="http://e-editiones.ch/ontology/document#hasFootnoteReference">
    <rdfs:range rdf:resource="http://e-editiones.ch/ontology/document#FootnoteReference"/>

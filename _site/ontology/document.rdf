        <rdfs:domain rdf:resource="http://e-editiones.ch/ontology/document#FootnoteReference"/>
        <rdfs:label xml:lang="de">hat Fussnoteidentifikator</rdfs:label>
        <rdfs:label xml:lang="en">has footnote identifier</rdfs:label>
        <rdfs:comment xml:lang="en">Relating a footnote reference to its footnote identifier.</rdfs:comment>
        <rdfs:range rdf:resource="http://e-editiones.ch/ontology/note-structure#FootnoteIdentifier"/>

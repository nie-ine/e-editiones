    <rdfs:subClassOf rdf:resource="http://e-editiones.ch/ontology/document#Space"/>
    <rdfs:label xml:lang="de">vertikaler Leerraum in einem Dokument</rdfs:label>
    <rdfs:comment xml:lang="en">Document space with vertical orientation, e.g. between columns.</rdfs:comment>
    <rdfs:label xml:lang="en">vertical document space</rdfs:label>
  </rdfs:Class>
  <rdfs:Class rdf:about="http://e-editiones.ch/ontology/document#TitlePage">
    <rdfs:label xml:lang="en">title page</rdfs:label>
    <rdfs:subClassOf rdf:resource="http://e-editiones.ch/ontology/document#Page"/>

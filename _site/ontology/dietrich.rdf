    <rdfs:label xml:lang="de">Tagebucheintrag des Paters Joseph Dietrich</rdfs:label>
    <rdfs:subClassOf rdf:resource="http://e-editiones.ch/ontology/text-expression#DiaryEntry"/>
    <rdfs:isDefinedBy rdf:resource="http://e-editiones.ch/ontology/dietrich#"/>
    <rdfs:label xml:lang="en">diary entry by father Joseph Dietrich</rdfs:label>
  </rdfs:Class>

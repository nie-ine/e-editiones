>
  <rdfs:Class rdf:about="http://e-editiones.ch/ontology/dietrich#DiaryVolume">
    <rdfs:subClassOf rdf:resource="http://e-editiones.ch/ontology/information-carrier#Manuscript"/>
    <rdfs:isDefinedBy rdf:resource="http://e-editiones.ch/ontology/dietrich#"/>
    <rdfs:label xml:lang="de">Tagebuch-Band des Paters Joseph Dietrich</rdfs:label>
    <rdfs:label xml:lang="en">diary volume by father Joseph Dietrich</rdfs:label>
    <rdfs:subClassOf rdf:resource="http://e-editiones.ch/ontology/information-carrier#Volume"/>
    <rdfs:subClassOf rdf:resource="http://e-editiones.ch/ontology/information-carrier#Diary"/>
    <rdfs:comment xml:lang="en">Diary volume written and authored by father Joseph Dietrich.</rdfs:comment>
  </rdfs:Class>
  <owl:DatatypeProperty rdf:about="http://e-editiones.ch/ontology/dietrich#personHasEinsiedlerProfessIdentifier">
    <rdfs:label xml:lang="de">Person hat Einsiedler-Eintrittidentifikator</rdfs:label>
    <rdfs:label xml:lang="en">person has Einsiedler profess identifier</rdfs:label>
    <rdfs:range rdf:resource="http://www.w3.org/2000/01/rdf-schema#Literal"/>
    <rdfs:comment xml:lang="en">Relating a catholic order person to an identifier for being professed into the order of the monastery of Einsiedeln.</rdfs:comment>
    <rdfs:domain rdf:resource="http://e-editiones.ch/ontology/catholic-order#Person"/>
    <rdfs:isDefinedBy rdf:resource="http://e-editiones.ch/ontology/dietrich#"/>
  </owl:DatatypeProperty>
  <owl:DatatypeProperty rdf:about="http://e-editiones.ch/ontology/dietrich#personHasNonEinsiedler-StGaller-PfaeferserProfessIdentifier">
    <rdfs:domain rdf:resource="http://e-editiones.ch/ontology/catholic-order#Person"/>
    <rdfs:label xml:lang="de">Person hat non-Einsiedler-StGaller-Pfaeferser-Eintrittidentifikator</rdfs:label>
    <rdfs:comment xml:lang="en">Relating a catholic order person to an identifier for being professed into the order of the monastery other than the one of Einsiedeln, St. Gallen, and Pfäfers.</rdfs:comment>
    <rdfs:label xml:lang="en">person has non-Einsiedler-StGaller-Pfaeferser profess identifier</rdfs:label>
    <rdfs:range rdf:resource="http://www.w3.org/2000/01/rdf-schema#Literal"/>
    <rdfs:isDefinedBy rdf:resource="http://e-editiones.ch/ontology/dietrich#"/>
  </owl:DatatypeProperty>
  <owl:DatatypeProperty rdf:about="http://e-editiones.ch/ontology/dietrich#personHasPfaeferserProfessIdentifier">
    <rdfs:range rdf:resource="http://www.w3.org/2000/01/rdf-schema#Literal"/>
    <rdfs:isDefinedBy rdf:resource="http://e-editiones.ch/ontology/dietrich#"/>
    <rdfs:comment xml:lang="en">Relating a catholic order person to an identifier for being professed into the order of the monastery of Pfäfers.</rdfs:comment>
    <rdfs:label xml:lang="de">Person hat St. Pfäferser-Eintrittidentifikator</rdfs:label>
    <rdfs:domain rdf:resource="http://e-editiones.ch/ontology/catholic-order#Person"/>
    <rdfs:label xml:lang="en">person has Pfäferser profess identifier</rdfs:label>
  </owl:DatatypeProperty>
  <owl:DatatypeProperty rdf:about="http://e-editiones.ch/ontology/dietrich#personHasStGallerProfessIdentifier">
    <rdfs:label xml:lang="de">Person hat St. Galler-Eintrittidentifikator</rdfs:label>
    <rdfs:range rdf:resource="http://www.w3.org/2000/01/rdf-schema#Literal"/>

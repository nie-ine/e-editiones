    <rdfs:isDefinedBy rdf:resource="http://e-editiones.ch/ontology/dietrich#"/>
  </owl:DatatypeProperty>
  <owl:Ontology rdf:about="http://e-editiones.ch/ontology/dietrich">
    <dct:creator xml:lang="en">Hans Cools, MD, knowledge engineer, ontologist, software developer, Basel University, Switzerland</dct:creator>
    <dct:contributor xml:lang="en">Reinhard Priber, tool coordinator and software developer, University of Bern, Switzerland</dct:contributor>
    <dct:title xml:lang="en">An ontology about father Joseph Dietrich's monastery diary</dct:title>
    <dct:description xml:lang="en">Formal description of specific concepts in the scientific study "Das Kloster-Tagebuch des Einsiedler Paters Joseph Dietrich, 1670-1704".</dct:description>
    <dct:publisher xml:lang="en">Universities of Basel, Bern, and Zürich, Switzerland</dct:publisher>
    <dct:license rdf:resource="http://creativecommons.org/licenses/by/3.0/"/>
    <owl:versionInfo rdf:datatype="http://www.w3.org/2001/XMLSchema#date">2020-06-11</owl:versionInfo>
  </owl:Ontology>
  <owl:ObjectProperty rdf:about="http://e-editiones.ch/ontology/dietrich#hasExternalResource">
    <rdfs:label xml:lang="de">hat externe Ressource</rdfs:label>
    <rdfs:range rdf:resource="http://www.w3.org/2000/01/rdf-schema#Resource"/>
    <rdfs:isDefinedBy rdf:resource="http://e-editiones.ch/ontology/dietrich#"/>
    <rdfs:domain rdf:resource="http://www.w3.org/2000/01/rdf-schema#Resource"/>
    <rdfs:comment xml:lang="en">Relating a Dietrich-project resource to an external, i.e. non-Dietrich-project resource.</rdfs:comment>
    <rdfs:label xml:lang="en">has external resource</rdfs:label>
  </owl:ObjectProperty>
  <owl:ObjectProperty rdf:about="http://e-editiones.ch/ontology/dietrich#personHasEinsiedlerAbbot">
    <rdfs:comment xml:lang="en">Relating a catholic order person to the person being the abbot of the monastery of Einsiedeln.</rdfs:comment>
    <rdfs:range>
      <owl:Restriction rdf:nodeID="ub35bL94C29">
        <owl:someValuesFrom rdf:resource="http://e-editiones.ch/ontology/catholic-order#Abbot"/>
        <owl:onProperty rdf:resource="http://e-editiones.ch/ontology/human#personHasRole"/>
      </owl:Restriction>
    </rdfs:range>
    <rdfs:domain rdf:resource="http://e-editiones.ch/ontology/catholic-order#Person"/>
    <rdfs:label xml:lang="en">person has Einsiedler abbot</rdfs:label>
    <rdfs:label xml:lang="de">Person hat Einsiedler-Abt</rdfs:label>
    <rdfs:isDefinedBy rdf:resource="http://e-editiones.ch/ontology/dietrich#"/>
    <rdfs:range rdf:resource="http://e-editiones.ch/ontology/catholic-order#Person"/>
  </owl:ObjectProperty>
  <rdfs:Class rdf:about="http://e-editiones.ch/ontology/dietrich#DiaryEntry">

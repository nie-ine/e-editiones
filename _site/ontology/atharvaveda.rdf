<?xml version="1.0" encoding="utf-8"?>
<rdf:RDF
  xmlns:dct="http://purl.org/dc/terms/"
  xmlns:rdfs="http://www.w3.org/2000/01/rdf-schema#"
  xmlns:owl="http://www.w3.org/2002/07/owl#"
  xmlns:skos="http://www.w3.org/2004/02/skos/core#"
  xmlns:rdf="http://www.w3.org/1999/02/22-rdf-syntax-ns#"
>
  <owl:ObjectProperty rdf:about="http://e-editiones.ch/ontology/atharvaveda#paippaladaHasGenre">
    <rdfs:domain rdf:resource="http://e-editiones.ch/ontology/atharvaveda#Paippalada-Samhita"/>
    <rdfs:label xml:lang="de">Paippalada hat Gattung</rdfs:label>
    <rdfs:comment xml:lang="en">Relating a Paippalada to its genre.</rdfs:comment>
    <rdfs:isDefinedBy rdf:resource="http://e-editiones.ch/ontology/atharvaveda#"/>
    <rdfs:range rdf:resource="http://e-editiones.ch/ontology/indology#Samhita"/>
    <rdfs:label xml:lang="en">Paippalada has genre</rdfs:label>
    <rdfs:subPropertyOf rdf:resource="http://e-editiones.ch/ontology/indology#vedaHasGenre"/>
  </owl:ObjectProperty>
  <owl:ObjectProperty rdf:about="http://e-editiones.ch/ontology/atharvaveda#isNormalizationOf">
    <rdfs:label xml:lang="en">is normalization of</rdfs:label>
    <rdfs:subPropertyOf rdf:resource="http://e-editiones.ch/ontology/text#isNormalizationOf"/>
    <rdfs:domain rdf:resource="http://e-editiones.ch/ontology/atharvaveda#Padapatha"/>
    <rdfs:label xml:lang="de">ist Normalisierung von</rdfs:label>
    <rdfs:comment xml:lang="en">Relating a normalized Padapatha to its Samhitapatha.</rdfs:comment>
    <rdfs:range rdf:resource="http://e-editiones.ch/ontology/indology#Samhitapatha"/>
  </owl:ObjectProperty>
  <owl:Ontology rdf:about="http://e-editiones.ch/ontology/atharvaveda">
    <dct:contributor xml:lang="en">Reto Baumgartner, tool coordinator and software developer, University of Zürich, Switzerland</dct:contributor>
    <dct:license rdf:resource="http://creativecommons.org/licenses/by/3.0/"/>
    <dct:publisher xml:lang="en">Universities of Basel, Bern, and Zürich, Switzerland</dct:publisher>
    <dct:description xml:lang="en">Formal description of specific concepts in the scientific study of the Atharvaveda.</dct:description>
    <dct:contributor xml:lang="en">Magdalena Plamada, PhD, Zürich University, Switzerland</dct:contributor>
    <owl:versionInfo rdf:datatype="http://www.w3.org/2001/XMLSchema#date">2020-06-11</owl:versionInfo>
    <dct:title xml:lang="en">An ontology about Atharvaveda</dct:title>
    <dct:creator xml:lang="en">Hans Cools, MD, knowledge engineer, ontologist, software developer, Basel University, Switzerland</dct:creator>
  </owl:Ontology>
  <owl:ObjectProperty rdf:about="http://e-editiones.ch/ontology/atharvaveda#isVerseOfHalfStrophe">
    <rdfs:label xml:lang="en">is verse of half-strophe</rdfs:label>
    <rdfs:subPropertyOf rdf:resource="http://e-editiones.ch/ontology/prosodic-structure#isVerseOfHalfStrophe"/>
    <rdfs:label xml:lang="de">ist Vers einer Halbstrophe</rdfs:label>
    <rdfs:range rdf:resource="http://e-editiones.ch/ontology/atharvaveda#HalfStrophe"/>
    <rdfs:comment xml:lang="en">Relating an Atharvaveda verse to a half-strophe it is part of.</rdfs:comment>
    <rdfs:domain rdf:resource="http://e-editiones.ch/ontology/atharvaveda#Verse"/>
    <rdfs:isDefinedBy rdf:resource="http://e-editiones.ch/ontology/atharvaveda#"/>
  </owl:ObjectProperty>
  <owl:ObjectProperty rdf:about="http://e-editiones.ch/ontology/atharvaveda#isVerseOfStrophe">
    <rdfs:domain rdf:resource="http://e-editiones.ch/ontology/atharvaveda#Verse"/>
    <rdfs:subPropertyOf rdf:resource="http://e-editiones.ch/ontology/prosodic-structure#isVerseOfStrophe"/>
    <rdfs:label xml:lang="de">ist Vers einer Strophe</rdfs:label>
    <rdfs:range rdf:resource="http://e-editiones.ch/ontology/atharvaveda#Strophe"/>
    <rdfs:comment xml:lang="en">Relating an Atharvaveda verse to a strophe it is part of.</rdfs:comment>
    <rdfs:label xml:lang="en">is verse of strophe</rdfs:label>
    <rdfs:isDefinedBy rdf:resource="http://e-editiones.ch/ontology/atharvaveda#"/>
  </owl:ObjectProperty>
  <owl:ObjectProperty rdf:about="http://e-editiones.ch/ontology/atharvaveda#paippaladaHasRecitingStyle">
    <rdfs:subPropertyOf rdf:resource="http://e-editiones.ch/ontology/indology#vedaHasRecitingStyle"/>
    <rdfs:label xml:lang="en">Paippalada has reciting style</rdfs:label>
    <rdfs:range rdf:resource="http://e-editiones.ch/ontology/indology#Samhitapatha"/>
    <rdfs:domain rdf:resource="http://e-editiones.ch/ontology/atharvaveda#Paippalada-Samhita"/>
    <rdfs:isDefinedBy rdf:resource="http://e-editiones.ch/ontology/atharvaveda#"/>
    <rdfs:comment xml:lang="en">Relating a Paippalada to a reciting style it has.</rdfs:comment>
    <rdfs:label xml:lang="de">Paippalada hat Rezitationsstil</rdfs:label>
  </owl:ObjectProperty>
  <owl:ObjectProperty rdf:about="http://e-editiones.ch/ontology/atharvaveda#isSuktaOfKanda">
    <rdfs:comment xml:lang="en">Relating an Atharvaveda sukta to its kanda.</rdfs:comment>
    <rdfs:label xml:lang="de">ist Sukta einer Kanda</rdfs:label>
    <rdfs:subPropertyOf rdf:resource="http://e-editiones.ch/ontology/literature#isVerseSongOfCollection"/>
    <rdfs:isDefinedBy rdf:resource="http://e-editiones.ch/ontology/atharvaveda#"/>
    <rdfs:range rdf:resource="http://e-editiones.ch/ontology/atharvaveda#Kanda"/>
    <rdfs:domain rdf:resource="http://e-editiones.ch/ontology/atharvaveda#Sukta"/>
    <rdfs:label xml:lang="en">is sukta of kanda</rdfs:label>
  </owl:ObjectProperty>
  <owl:ObjectProperty rdf:about="http://e-editiones.ch/ontology/atharvaveda#isStropheOfSukta">
    <rdfs:subPropertyOf rdf:resource="http://e-editiones.ch/ontology/prosodic-structure#isStropheOfPoem"/>
    <rdfs:isDefinedBy rdf:resource="http://e-editiones.ch/ontology/atharvaveda#"/>
    <rdfs:label xml:lang="de">ist Strophe einer Sukta</rdfs:label>
    <rdfs:label xml:lang="en">is strophe of sukta</rdfs:label>
    <rdfs:comment xml:lang="en">Relating an Atharvaveda strophe to its sukta.</rdfs:comment>
    <rdfs:domain rdf:resource="http://e-editiones.ch/ontology/atharvaveda#Strophe"/>
    <rdfs:range rdf:resource="http://e-editiones.ch/ontology/atharvaveda#Sukta"/>
  </owl:ObjectProperty>
  <owl:ObjectProperty rdf:about="http://e-editiones.ch/ontology/atharvaveda#isKandaOfAtharvaveda">
    <rdfs:domain rdf:resource="http://e-editiones.ch/ontology/atharvaveda#Kanda"/>
    <rdfs:subPropertyOf rdf:resource="http://e-editiones.ch/ontology/concept#isPartOf"/>
    <rdfs:label xml:lang="en">is kanda of Atharvaveda</rdfs:label>
    <rdfs:comment xml:lang="en">Relating a kanda to its Atharvaveda.</rdfs:comment>
    <rdfs:label xml:lang="de">ist Kanda einer Atharvaveda</rdfs:label>
    <rdfs:range rdf:resource="http://e-editiones.ch/ontology/atharvaveda#Atharvaveda"/>
    <rdfs:isDefinedBy rdf:resource="http://e-editiones.ch/ontology/atharvaveda#"/>
  </owl:ObjectProperty>
  <rdfs:Class rdf:about="http://e-editiones.ch/ontology/atharvaveda#Paippalada-Samhita">
    <rdfs:label xml:lang="sa">Paippalāda-Samhitā</rdfs:label>
    <rdfs:label xml:lang="de">Paippalada-Samhita</rdfs:label>
    <rdfs:subClassOf rdf:resource="http://e-editiones.ch/ontology/atharvaveda#Atharvaveda"/>
    <rdfs:label xml:lang="en">Paippalada-Samhita</rdfs:label>
    <rdfs:isDefinedBy rdf:resource="http://e-editiones.ch/ontology/atharvaveda#"/>
    <rdfs:comment xml:lang="en">One of the nine recensions of the Atharvaveda, along the different shakhas or Hindu theological schools, specialized in learning Vedic texts. Only this one and the Shaunakiya recension are extent.</rdfs:comment>
    <rdfs:subClassOf rdf:resource="http://e-editiones.ch/ontology/text-editing#Recension"/>
  </rdfs:Class>
  <rdfs:Class rdf:about="http://e-editiones.ch/ontology/atharvaveda#Kanda">
    <rdfs:subClassOf rdf:resource="http://e-editiones.ch/ontology/literature#VerseSongCollection"/>
    <rdfs:comment xml:lang="en">Kanda of the Atharvaveda.</rdfs:comment>
    <rdfs:label xml:lang="sa">Atharvaveda kāṇḍa</rdfs:label>
    <skos:note xml:lang="en">The Atharvaveda contains 20 Kandas.</skos:note>
    <rdfs:label xml:lang="en">Atharvaveda kanda</rdfs:label>
    <rdfs:isDefinedBy rdf:resource="http://e-editiones.ch/ontology/atharvaveda#"/>
    <rdfs:subClassOf rdf:resource="http://e-editiones.ch/ontology/indology#Expression"/>
    <rdfs:label xml:lang="de">Atharvaveda Kanda</rdfs:label>
  </rdfs:Class>
  <rdfs:Class rdf:about="http://e-editiones.ch/ontology/atharvaveda#Sukta">
    <rdfs:subClassOf rdf:resource="http://e-editiones.ch/ontology/literature#VerseSong"/>
    <rdfs:comment xml:lang="en">Sukta in the Atharvaveda.</rdfs:comment>
    <skos:note xml:lang="en">The Atharvaveda contains 730 Suktas.</skos:note>
    <rdfs:subClassOf rdf:resource="http://e-editiones.ch/ontology/indology#Expression"/>
    <rdfs:isDefinedBy rdf:resource="http://e-editiones.ch/ontology/atharvaveda#"/>
    <rdfs:label xml:lang="de">Atharvaveda-Sukta</rdfs:label>
    <rdfs:label xml:lang="en">Atharvaveda sukta</rdfs:label>
    <rdfs:subClassOf rdf:resource="http://e-editiones.ch/ontology/text#HandwrittenText"/>
    <rdfs:label xml:lang="sa">Atharvaveda sūkta</rdfs:label>
  </rdfs:Class>
  <rdfs:Class rdf:about="http://e-editiones.ch/ontology/atharvaveda#HalfStrophe">
    <rdfs:comment xml:lang="en">Half a strophe in the Atharvaveda.</rdfs:comment>
    <rdfs:subClassOf rdf:resource="http://e-editiones.ch/ontology/prosodic-structure#HalfStrophe"/>
    <rdfs:label xml:lang="de">Atharvaveda-Halbstrophe</rdfs:label>
    <rdfs:label xml:lang="en">Atharvaveda half-strophe</rdfs:label>
    <rdfs:isDefinedBy rdf:resource="http://e-editiones.ch/ontology/atharvaveda#"/>
  </rdfs:Class>
  <rdfs:Class rdf:about="http://e-editiones.ch/ontology/atharvaveda#Padapatha">
    <rdfs:subClassOf rdf:resource="http://e-editiones.ch/ontology/indology#Padapatha"/>
    <rdfs:label xml:lang="de">Normalisierte Padapatha</rdfs:label>
    <rdfs:comment xml:lang="en">Padapatha normalized from a Samhitapatha.</rdfs:comment>
    <rdfs:label xml:lang="en">Normalized Padapatha</rdfs:label>
    <rdfs:isDefinedBy rdf:resource="http://e-editiones.ch/ontology/atharvaveda#"/>
    <rdfs:subClassOf rdf:resource="http://e-editiones.ch/ontology/text#Normalization"/>
  </rdfs:Class>
  <rdfs:Class rdf:about="http://e-editiones.ch/ontology/atharvaveda#Verse">
    <rdfs:comment xml:lang="en">Verse as part of a strophe in the Atharvaveda.</rdfs:comment>
    <rdfs:label xml:lang="en">Atharvaveda verse</rdfs:label>
    <rdfs:subClassOf rdf:resource="http://e-editiones.ch/ontology/prosodic-structure#Verse"/>
    <rdfs:isDefinedBy rdf:resource="http://e-editiones.ch/ontology/atharvaveda#"/>
    <skos:note xml:lang="en">The Atharvaveda contains about 6000 verses.</skos:note>
    <rdfs:label xml:lang="sa">Atharvaveda pada</rdfs:label>
    <rdfs:label xml:lang="de">Atharvaveda-Vers</rdfs:label>
  </rdfs:Class>
  <rdfs:Class rdf:about="http://e-editiones.ch/ontology/atharvaveda#Strophe">
    <rdfs:subClassOf rdf:resource="http://e-editiones.ch/ontology/text-expression#ExpressionPart"/>
    <rdfs:label xml:lang="en">Atharvaveda strophe</rdfs:label>
    <rdfs:label xml:lang="de">Atharvaveda-Strophe</rdfs:label>
    <rdfs:comment xml:lang="en">Group of verses or 2 half-strophes in the Atharvaveda.</rdfs:comment>
    <rdfs:subClassOf rdf:resource="http://e-editiones.ch/ontology/prosodic-structure#Strophe"/>
    <rdfs:isDefinedBy rdf:resource="http://e-editiones.ch/ontology/atharvaveda#"/>
  </rdfs:Class>
  <rdfs:Class rdf:about="http://e-editiones.ch/ontology/atharvaveda#Atharvaveda">
    <skos:note xml:lang="en">The Atharvaveda contains 20 Kandas.</skos:note>
    <rdfs:isDefinedBy rdf:resource="http://e-editiones.ch/ontology/atharvaveda#"/>
    <rdfs:label xml:lang="sa">Atharvaveda</rdfs:label>
    <rdfs:label xml:lang="en">Atharvaveda</rdfs:label>
    <rdfs:subClassOf rdf:resource="http://e-editiones.ch/ontology/indology#Veda"/>
    <rdfs:label xml:lang="de">Atharvaveda</rdfs:label>
    <rdfs:comment xml:lang="en">Fourth Veda, containing the knowledge of atharvāṇas, the procedures of beliefs and rituals addressing practical issues of daily life of the Vedic society in Hinduism, compiled about 1200 to 1000 BC.</rdfs:comment>
  </rdfs:Class>
</rdf:RDF>

<?xml version="1.0" encoding="utf-8"?>
<rdf:RDF
  xmlns:owl="http://www.w3.org/2002/07/owl#"
  xmlns:rdf="http://www.w3.org/1999/02/22-rdf-syntax-ns#"
  xmlns:rdfs="http://www.w3.org/2000/01/rdf-schema#"
  xmlns:dct="http://purl.org/dc/terms/"
>
  <rdfs:Class rdf:about="http://e-editiones.ch/ontology/human-geography#SovereignState">
    <rdfs:isDefinedBy rdf:resource="http://e-editiones.ch/ontology/human-geography#"/>
    <rdfs:comment xml:lang="en">Political organization of a sovereign state with an own government.</rdfs:comment>
    <rdfs:label xml:lang="en">sovereign state</rdfs:label>
    <rdfs:subClassOf rdf:resource="http://e-editiones.ch/ontology/human-geography#State"/>
    <rdfs:label xml:lang="de">Souveränstaat</rdfs:label>
  </rdfs:Class>
  <rdfs:Class rdf:about="http://e-editiones.ch/ontology/human-geography#Country">
    <rdfs:subClassOf rdf:resource="http://e-editiones.ch/ontology/physical-entity#EarthPlace"/>
    <rdfs:comment xml:lang="en">Place of a nation.</rdfs:comment>
    <rdfs:label xml:lang="en">country</rdfs:label>
    <rdfs:isDefinedBy rdf:resource="http://e-editiones.ch/ontology/human-geography#"/>
    <rdfs:label xml:lang="de">Land</rdfs:label>
  </rdfs:Class>
  <rdfs:Class rdf:about="http://e-editiones.ch/ontology/human-geography#StreetPlace">
    <rdfs:label xml:lang="en">street place</rdfs:label>
    <rdfs:comment xml:lang="en">.</rdfs:comment>
    <rdfs:isDefinedBy rdf:resource="http://e-editiones.ch/ontology/human-geography#"/>
    <rdfs:label xml:lang="de">Strassenort</rdfs:label>
    <rdfs:subClassOf rdf:resource="http://e-editiones.ch/ontology/physical-entity#EarthPlace"/>
  </rdfs:Class>
  <rdfs:Class rdf:about="http://e-editiones.ch/ontology/human-geography#Town">
    <rdfs:subClassOf rdf:resource="http://e-editiones.ch/ontology/physical-entity#EarthPlace"/>
    <rdfs:isDefinedBy rdf:resource="http://e-editiones.ch/ontology/human-geography#"/>
    <rdfs:label xml:lang="de">Ortschaft</rdfs:label>
    <rdfs:comment xml:lang="en">Smallest place with political boundaries and an own government.</rdfs:comment>
    <rdfs:label xml:lang="en">town</rdfs:label>
  </rdfs:Class>
  <owl:ObjectProperty rdf:about="http://e-editiones.ch/ontology/human-geography#hasReceiverPostalAddress">
    <rdfs:comment xml:lang="en">Relating something being sent to the postal address of the receiver.</rdfs:comment>
    <rdfs:isDefinedBy rdf:resource="http://e-editiones.ch/ontology/human-geography#"/>
    <rdfs:domain rdf:resource="http://e-editiones.ch/ontology/physical-entity#Matter"/>
    <rdfs:range rdf:resource="http://e-editiones.ch/ontology/human-geography#PostalAddress"/>
    <rdfs:label xml:lang="en">has receiver postal address</rdfs:label>
    <rdfs:label xml:lang="de">hat Empfängerpostanschrift</rdfs:label>
  </owl:ObjectProperty>
  <owl:DatatypeProperty rdf:about="http://e-editiones.ch/ontology/human-geography#addressHasAddresseeNameLiteral">
    <rdfs:subPropertyOf rdf:resource="http://e-editiones.ch/ontology/text#hasNameLiteral"/>
    <rdfs:label xml:lang="de">Postanschrift hat Namenliteral des Adressats</rdfs:label>
    <rdfs:isDefinedBy rdf:resource="http://e-editiones.ch/ontology/human-geography#"/>
    <rdfs:range rdf:resource="http://www.w3.org/2000/01/rdf-schema#Literal"/>
    <rdfs:domain rdf:resource="http://e-editiones.ch/ontology/human-geography#PostalAddress"/>
    <rdfs:label xml:lang="en">postal address has addressee name literal</rdfs:label>
    <rdfs:comment xml:lang="en">Relating a postal address to an addressee name literal.</rdfs:comment>
  </owl:DatatypeProperty>
  <rdfs:Class rdf:about="http://e-editiones.ch/ontology/human-geography#Nation">
    <rdfs:subClassOf rdf:resource="http://e-editiones.ch/ontology/organization#PoliticalOrganization"/>
    <rdfs:isDefinedBy rdf:resource="http://e-editiones.ch/ontology/human-geography#"/>
    <rdfs:label xml:lang="de">Nation</rdfs:label>
    <rdfs:comment xml:lang="en">Stable political organization with a commonality e.g. ethnicity, language, culture, .</rdfs:comment>
    <rdfs:label xml:lang="en">nation</rdfs:label>
  </rdfs:Class>
  <rdfs:Class rdf:about="http://e-editiones.ch/ontology/human-geography#District">
    <rdfs:label xml:lang="de">Stadtviertel</rdfs:label>
    <rdfs:label xml:lang="en">district</rdfs:label>
    <rdfs:comment xml:lang="en">Part of a city with own administration.</rdfs:comment>
    <rdfs:subClassOf rdf:resource="http://e-editiones.ch/ontology/physical-entity#EarthPlace"/>
    <rdfs:isDefinedBy rdf:resource="http://e-editiones.ch/ontology/human-geography#"/>
  </rdfs:Class>
  <owl:ObjectProperty rdf:about="http://e-editiones.ch/ontology/human-geography#hasDomicile">
    <rdfs:label xml:lang="de">hat Sitz</rdfs:label>
    <rdfs:subPropertyOf rdf:resource="http://e-editiones.ch/ontology/physical-entity#hasLocation"/>
    <rdfs:range rdf:resource="http://e-editiones.ch/ontology/physical-entity#Place"/>
    <rdfs:domain rdf:resource="http://e-editiones.ch/ontology/human#Agent"/>
    <rdfs:comment xml:lang="en">Relating a human agent - person or group - to a place of registered staying and contactability.</rdfs:comment>
    <rdfs:label xml:lang="en">has domicile</rdfs:label>
    <rdfs:isDefinedBy rdf:resource="http://e-editiones.ch/ontology/human-geography#"/>
  </owl:ObjectProperty>
  <rdfs:Class rdf:about="http://e-editiones.ch/ontology/human-geography#Emperor">
    <rdfs:label xml:lang="en">emperor</rdfs:label>
    <rdfs:label xml:lang="fr">empereur</rdfs:label>
    <rdfs:subClassOf rdf:resource="http://e-editiones.ch/ontology/human#PersonRole"/>
    <rdfs:isDefinedBy rdf:resource="http://e-editiones.ch/ontology/human-geography#"/>
    <rdfs:label xml:lang="de">Kaiser</rdfs:label>
    <rdfs:comment xml:lang="en">Role of a man being a sovereign head of an empire or husband of a woman being a sovereign head of an empire.</rdfs:comment>
  </rdfs:Class>
  <owl:DatatypeProperty rdf:about="http://e-editiones.ch/ontology/human-geography#addressHasHouseNumberNumeral">
    <rdfs:label xml:lang="de">Postanschrift hat Hausnummernumeral</rdfs:label>
    <rdfs:label xml:lang="en">postal address has house number numeral</rdfs:label>
    <rdfs:isDefinedBy rdf:resource="http://e-editiones.ch/ontology/human-geography#"/>
    <rdfs:subPropertyOf rdf:resource="http://e-editiones.ch/ontology/mathematics#hasOrdinalNumeral"/>
    <rdfs:domain rdf:resource="http://e-editiones.ch/ontology/human-geography#PostalAddress"/>
    <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#positiveInteger"/>
    <rdfs:comment xml:lang="en">Relating a postal address to the nummer numeral of the house.</rdfs:comment>
  </owl:DatatypeProperty>
  <rdfs:Class rdf:about="http://e-editiones.ch/ontology/human-geography#King">
    <rdfs:subClassOf rdf:resource="http://e-editiones.ch/ontology/human#Monarch"/>
    <rdfs:label xml:lang="fr">roi</rdfs:label>
    <rdfs:comment xml:lang="en">Role of a man being the absolute ruler in a monarchy or husband of a woman being the absolute ruler in a monarchy.</rdfs:comment>
    <rdfs:label xml:lang="de">König</rdfs:label>
    <rdfs:isDefinedBy rdf:resource="http://e-editiones.ch/ontology/human-geography#"/>
    <rdfs:label xml:lang="en">king</rdfs:label>
  </rdfs:Class>
  <rdfs:Class rdf:about="http://e-editiones.ch/ontology/human-geography#City">
    <rdfs:subClassOf rdf:resource="http://e-editiones.ch/ontology/physical-entity#EarthPlace"/>
    <rdfs:label xml:lang="de">Stadt</rdfs:label>
    <rdfs:isDefinedBy rdf:resource="http://e-editiones.ch/ontology/human-geography#"/>
    <rdfs:label xml:lang="en">city</rdfs:label>
    <rdfs:comment xml:lang="en">Large town.</rdfs:comment>
  </rdfs:Class>
  <rdfs:Class rdf:about="http://e-editiones.ch/ontology/human-geography#NationState">
    <rdfs:label xml:lang="en">nation state</rdfs:label>
    <rdfs:comment xml:lang="en">Political organization of a nation with an own government.</rdfs:comment>
    <rdfs:isDefinedBy rdf:resource="http://e-editiones.ch/ontology/human-geography#"/>
    <rdfs:subClassOf rdf:resource="http://e-editiones.ch/ontology/human-geography#State"/>
    <rdfs:label xml:lang="de">Nationalstaat</rdfs:label>
  </rdfs:Class>
  <owl:DatatypeProperty rdf:about="http://e-editiones.ch/ontology/human-geography#addressHasStreetNameLiteral">
    <rdfs:range rdf:resource="http://www.w3.org/2000/01/rdf-schema#Literal"/>
    <rdfs:comment xml:lang="en">Relating a postal address to the name literal of the street.</rdfs:comment>
    <rdfs:label xml:lang="en">postal address has street name literal</rdfs:label>
    <rdfs:domain rdf:resource="http://e-editiones.ch/ontology/human-geography#PostalAddress"/>
    <rdfs:subPropertyOf rdf:resource="http://e-editiones.ch/ontology/text#hasNameLiteral"/>
    <rdfs:label xml:lang="de">Postanschrift hat Strassennamenliteral</rdfs:label>
    <rdfs:isDefinedBy rdf:resource="http://e-editiones.ch/ontology/human-geography#"/>
  </owl:DatatypeProperty>
  <rdfs:Class rdf:about="http://e-editiones.ch/ontology/human-geography#Government">
    <rdfs:subClassOf rdf:resource="http://e-editiones.ch/ontology/organization#PoliticalOrganization"/>
    <rdfs:comment xml:lang="en">One or more persons in a politician role exercising authority over a particular territory.</rdfs:comment>
    <rdfs:label xml:lang="en">government</rdfs:label>
    <rdfs:label xml:lang="de">Regierung</rdfs:label>
    <rdfs:isDefinedBy rdf:resource="http://e-editiones.ch/ontology/human-geography#"/>
  </rdfs:Class>
  <owl:DatatypeProperty rdf:about="http://e-editiones.ch/ontology/human-geography#addressHasCountryNameLiteral">
    <rdfs:range rdf:resource="http://www.w3.org/2000/01/rdf-schema#Literal"/>
    <rdfs:label xml:lang="en">postal address has country name literal</rdfs:label>
    <rdfs:subPropertyOf rdf:resource="http://e-editiones.ch/ontology/text#hasNameLiteral"/>
    <rdfs:domain rdf:resource="http://e-editiones.ch/ontology/human-geography#PostalAddress"/>
    <rdfs:isDefinedBy rdf:resource="http://e-editiones.ch/ontology/human-geography#"/>
    <rdfs:comment xml:lang="en">Relating a postal address to its country name literal.</rdfs:comment>
    <rdfs:label xml:lang="de">Postanschrift hat Ländernamenliteral</rdfs:label>
  </owl:DatatypeProperty>
  <owl:ObjectProperty rdf:about="http://e-editiones.ch/ontology/human-geography#hasPostalAddress">
    <rdfs:comment xml:lang="en">Relating someone or something to her/his/its postal address.</rdfs:comment>
    <rdfs:isDefinedBy rdf:resource="http://e-editiones.ch/ontology/human-geography#"/>
    <rdfs:domain rdf:resource="http://www.w3.org/2000/01/rdf-schema#Resource"/>
    <rdfs:range rdf:resource="http://e-editiones.ch/ontology/human-geography#PostalAddress"/>
    <rdfs:label xml:lang="de">hat Postanschrift</rdfs:label>
    <rdfs:label xml:lang="en">has postal address</rdfs:label>
  </owl:ObjectProperty>
  <rdfs:Class rdf:about="http://e-editiones.ch/ontology/human-geography#Empress">
    <rdfs:isDefinedBy rdf:resource="http://e-editiones.ch/ontology/human-geography#"/>
    <rdfs:label xml:lang="fr">impératrice</rdfs:label>
    <rdfs:label xml:lang="en">empress</rdfs:label>
    <rdfs:label xml:lang="de">Kaiserin</rdfs:label>
    <rdfs:comment xml:lang="en">Role of a woman being a sovereign head of an empire or wife of a man being a sovereign head of an empire.</rdfs:comment>
    <rdfs:subClassOf rdf:resource="http://e-editiones.ch/ontology/human#PersonRole"/>
  </rdfs:Class>
  <rdfs:Class rdf:about="http://e-editiones.ch/ontology/human-geography#Village">
    <rdfs:subClassOf rdf:resource="http://e-editiones.ch/ontology/physical-entity#EarthPlace"/>
    <rdfs:isDefinedBy rdf:resource="http://e-editiones.ch/ontology/human-geography#"/>
    <rdfs:comment xml:lang="en">Small place with political boundaries but without an own government.</rdfs:comment>
    <rdfs:label xml:lang="en">village</rdfs:label>
    <rdfs:label xml:lang="de">Dorf</rdfs:label>
  </rdfs:Class>
  <rdfs:Class rdf:about="http://e-editiones.ch/ontology/human-geography#Monarchy">
    <rdfs:isDefinedBy rdf:resource="http://e-editiones.ch/ontology/human-geography#"/>
    <rdfs:label xml:lang="en">monarchy</rdfs:label>
    <rdfs:subClassOf rdf:resource="http://e-editiones.ch/ontology/human-geography#State"/>
    <rdfs:comment xml:lang="en">State wherein one person or family governs.</rdfs:comment>
    <rdfs:label xml:lang="de">Monarchie</rdfs:label>
  </rdfs:Class>
  <owl:ObjectProperty rdf:about="http://e-editiones.ch/ontology/human-geography#hasReturnPostalAddress">
    <rdfs:label xml:lang="de">hat Absenderpostanschrift</rdfs:label>
    <rdfs:comment xml:lang="en">Relating something being sent to the postal address for return.</rdfs:comment>
    <rdfs:isDefinedBy rdf:resource="http://e-editiones.ch/ontology/human-geography#"/>
    <rdfs:domain rdf:resource="http://e-editiones.ch/ontology/physical-entity#Matter"/>
    <rdfs:label xml:lang="en">has return postal address</rdfs:label>
    <rdfs:range rdf:resource="http://e-editiones.ch/ontology/human-geography#PostalAddress"/>
  </owl:ObjectProperty>
  <owl:DatatypeProperty rdf:about="http://e-editiones.ch/ontology/human-geography#addressHasTownNameLiteral">
    <rdfs:subPropertyOf rdf:resource="http://e-editiones.ch/ontology/text#hasNameLiteral"/>
    <rdfs:range rdf:resource="http://www.w3.org/2000/01/rdf-schema#Literal"/>
    <rdfs:domain rdf:resource="http://e-editiones.ch/ontology/human-geography#PostalAddress"/>
    <rdfs:isDefinedBy rdf:resource="http://e-editiones.ch/ontology/human-geography#"/>
    <rdfs:comment xml:lang="en">Relating a postal address to its town name literal.</rdfs:comment>
    <rdfs:label xml:lang="en">postal address has town name literal</rdfs:label>
    <rdfs:label xml:lang="de">Postanschrift hat Ortsnamenliteral</rdfs:label>
  </owl:DatatypeProperty>
  <owl:DatatypeProperty rdf:about="http://e-editiones.ch/ontology/human-geography#addressHasPostcodeValue">
    <rdfs:domain rdf:resource="http://e-editiones.ch/ontology/human-geography#PostalAddress"/>
    <rdfs:comment xml:lang="en">Relating a postal address to its postcode value.</rdfs:comment>
    <rdfs:label xml:lang="de">Postanschrift hat Postleitzahlwert</rdfs:label>
    <rdfs:isDefinedBy rdf:resource="http://e-editiones.ch/ontology/human-geography#"/>
    <rdfs:range rdf:resource="http://www.w3.org/2000/01/rdf-schema#Literal"/>
    <rdfs:label xml:lang="en">postal address has postcode value</rdfs:label>
    <rdfs:subPropertyOf rdf:resource="http://e-editiones.ch/ontology/concept#hasLiteral"/>
  </owl:DatatypeProperty>
  <rdfs:Class rdf:about="http://e-editiones.ch/ontology/human-geography#Queen">
    <rdfs:subClassOf rdf:resource="http://e-editiones.ch/ontology/human#Monarch"/>
    <rdfs:isDefinedBy rdf:resource="http://e-editiones.ch/ontology/human-geography#"/>
    <rdfs:label xml:lang="en">queen</rdfs:label>
    <rdfs:comment xml:lang="en">Role of a woman being the absolute ruler in a monarchy or wife of a man being the absolute ruler in a monarchy.</rdfs:comment>
    <rdfs:label xml:lang="fr">reine</rdfs:label>
    <rdfs:label xml:lang="de">Königin</rdfs:label>
  </rdfs:Class>
  <rdfs:Class rdf:about="http://e-editiones.ch/ontology/human-geography#FederalState">
    <rdfs:isDefinedBy rdf:resource="http://e-editiones.ch/ontology/human-geography#"/>
    <rdfs:subClassOf rdf:resource="http://e-editiones.ch/ontology/human-geography#State"/>
    <rdfs:label xml:lang="en">federal state</rdfs:label>
    <rdfs:label xml:lang="de">Bundesstaat</rdfs:label>
    <rdfs:comment xml:lang="en">Political organization of a federal state with an own government.</rdfs:comment>
  </rdfs:Class>
  <rdfs:Class rdf:about="http://e-editiones.ch/ontology/human-geography#Empire">
    <rdfs:label xml:lang="en">empire</rdfs:label>
    <rdfs:subClassOf rdf:resource="http://e-editiones.ch/ontology/organization#PoliticalOrganization"/>
    <rdfs:comment xml:lang="en">Group of nations with an overarching government.</rdfs:comment>
    <rdfs:isDefinedBy rdf:resource="http://e-editiones.ch/ontology/human-geography#"/>
    <rdfs:label xml:lang="de">Imperium</rdfs:label>
  </rdfs:Class>
  <rdfs:Class rdf:about="http://e-editiones.ch/ontology/human-geography#Monarch">
    <rdfs:label xml:lang="en">monarch</rdfs:label>
    <rdfs:subClassOf rdf:resource="http://e-editiones.ch/ontology/human#PersonRole"/>
    <rdfs:label xml:lang="fr">monarque</rdfs:label>
    <rdfs:isDefinedBy rdf:resource="http://e-editiones.ch/ontology/human-geography#"/>
    <rdfs:comment xml:lang="en">Role of a person being a sovereign head of state in a monarchy.</rdfs:comment>
    <rdfs:label xml:lang="de">Monarch</rdfs:label>
  </rdfs:Class>
  <rdfs:Class rdf:about="http://e-editiones.ch/ontology/human-geography#Street">
    <rdfs:isDefinedBy rdf:resource="http://e-editiones.ch/ontology/human-geography#"/>
    <rdfs:label xml:lang="en">street</rdfs:label>
    <rdfs:label xml:lang="de">Strasse</rdfs:label>
    <rdfs:comment xml:lang="en">.</rdfs:comment>
    <rdfs:subClassOf rdf:resource="http://e-editiones.ch/ontology/physical-entity#EarthPlace"/>
  </rdfs:Class>

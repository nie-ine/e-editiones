  <rdfs:Class rdf:about="http://e-editiones.ch/ontology/human-geography#PostalAddress">
    <rdfs:label xml:lang="de">Postanschrift</rdfs:label>
    <rdfs:subClassOf rdf:resource="http://e-editiones.ch/ontology/concept#SymbolicEntity"/>
    <rdfs:comment xml:lang="en">.</rdfs:comment>
    <rdfs:isDefinedBy rdf:resource="http://e-editiones.ch/ontology/human-geography#"/>
    <rdfs:label xml:lang="en">postal address</rdfs:label>
  </rdfs:Class>
  <rdfs:Class rdf:about="http://e-editiones.ch/ontology/human-geography#State">
    <rdfs:label xml:lang="en">state</rdfs:label>
    <rdfs:label xml:lang="de">Staat</rdfs:label>
    <rdfs:subClassOf rdf:resource="http://e-editiones.ch/ontology/organization#PoliticalOrganization"/>
    <rdfs:comment xml:lang="en">Political organization with an own government.</rdfs:comment>
    <rdfs:isDefinedBy rdf:resource="http://e-editiones.ch/ontology/human-geography#"/>
  </rdfs:Class>
